{
  "name": "musenest",
  "version": "1.0.0",
  "description": "Professional model portfolio management system built with Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "migrate:extra": "node scripts/migrate-extra.js",
    "migrate-data": "node scripts/migrate-data.js",
    "seed": "node scripts/seed.js",
    "verify-db": "node scripts/verify-db.js",
    "route-audit": "node scripts/route-audit.js",
    "verify-migrations": "node scripts/verify-migrations.js",
    "verify-api-mounts": "node scripts/verify-api-mounts.js",
    "smoke:api": "node scripts/smoke-api.js",
    "check": "npm run verify-db && npm run route-audit && npm run verify-migrations && npm run verify-api-mounts && npm run smoke:api",
    "ai:smoke": "node tests/api/test-ai-integration.js",
    "backfill:gallery": "node scripts/backfill_gallery.js $MODEL_SLUG",
    "uploads:normalize": "node scripts/normalize_uploads.js $MODEL_SLUG"
  },
  "keywords": [
    "model",
    "portfolio",
    "cms",
    "escort",
    "booking"
  ],
  "author": "MuseNest",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.3",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-fetch": "^3.3.2",
    "sharp": "^0.32.6",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
