                <a href="/{{../modelSlug}}/contact?package={{this.slug}}{{../previewParam}}" 
                   class="block w-full theme-accent-bg theme-text-inverted py-4 px-6 rounded-lg text-center font-semibold hover:opacity-90 transition-opacity">
                    Book {{this.name}}
                </a>
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/if}}

