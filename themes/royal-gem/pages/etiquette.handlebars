<!-- Royal Gem Etiquette Page - Universal System Integration -->
<section class="royal-section" style="background: linear-gradient(135deg, var(--theme-bg), var(--theme-surface)); position: relative;">
    <!-- Royal Decorative Elements -->
    <div style="position: absolute; top: 10%; left: 5%; opacity: 0.3; z-index: 1;">
        <svg class="royal-icon-lg" viewBox="0 0 64 64" style="animation: float 9s ease-in-out infinite; width: 2.5rem; height: 2.5rem;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#insignia"></use>
        </svg>
    </div>
    <div style="position: absolute; top: 60%; right: 8%; opacity: 0.4; z-index: 1;">
        <svg class="royal-icon" viewBox="0 0 64 64" style="animation: float 7s ease-in-out infinite; animation-delay: 3s;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#scepter"></use>
        </svg>
    </div>
    <div style="position: absolute; bottom: 15%; left: 90%; opacity: 0.5; z-index: 1;">
        <svg class="royal-icon" viewBox="0 0 64 64" style="animation: float 11s ease-in-out infinite; animation-delay: 5s;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#royal"></use>
        </svg>
    </div>

    <div class="royal-container" style="position: relative; z-index: 2;">
        <!-- Page Header -->
        {{#if etiquette_content.etiquette_header_visible}}
        <div style="text-align: center; margin-bottom: 4rem;">
            <div style="margin-bottom: 2rem;">
                <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); animation: pulse 3s ease-in-out infinite;">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
                </svg>
            </div>
            <h1 style="background: linear-gradient(45deg, var(--theme-accent), var(--theme-text)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text;">
                {{#if etiquette_content.page_title}}{{etiquette_content.page_title}}{{else}}Etiquette & Guidelines{{/if}}
            </h1>
            {{#if etiquette_content.page_subtitle}}
            <p style="font-size: 1.2rem; color: var(--theme-text-subtle); max-width: 700px; margin: 0 auto;">
                {{etiquette_content.page_subtitle}}
            </p>
            {{/if}}
        </div>
        {{else}}
        <!-- Default Header -->
        <div style="text-align: center; margin-bottom: 4rem;">
            <div style="margin-bottom: 2rem;">
                <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); animation: pulse 3s ease-in-out infinite;">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
                </svg>
            </div>
            <h1 style="background: linear-gradient(45deg, var(--theme-accent), var(--theme-text)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text;">
                Etiquette & Guidelines
            </h1>
            <p style="font-size: 1.2rem; color: var(--theme-text-subtle); max-width: 700px; margin: 0 auto;">
                Guidelines to ensure our time together is respectful, comfortable, and memorable for both of us.
            </p>
        </div>
        {{/if}}

        <!-- 2x2 Grid Layout -->
        <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(500px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
            
            <!-- Booking & Screening -->
            {{#if etiquette_content.etiquette_booking_visible}}
            <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-surface), var(--theme-primary)); position: relative; overflow: hidden;">
                <div style="position: absolute; top: -20px; right: -20px; opacity: 0.2; z-index: 1;">
                    <svg style="width: 80px; height: 80px; color: var(--theme-accent);" viewBox="0 0 64 64">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
                    </svg>
                </div>
                
                <div style="position: relative; z-index: 2;">
                    <div style="display: flex; align-items: center; gap: 0.8rem; margin-bottom: 2rem; padding-bottom: 1rem; border-bottom: 2px solid var(--theme-accent);">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#calendar"></use>
                        </svg>
                        <h2 style="color: var(--theme-accent); font-size: 1.5rem;">
                            {{#if etiquette_content.booking_title}}{{etiquette_content.booking_title}}{{else}}Booking & Screening{{/if}}
                        </h2>
                    </div>
                    
                    <div style="display: grid; gap: 1.5rem;">
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#sapphire"></use>
                                </svg>
                                {{#if etiquette_content.booking_initial_contact_title}}{{etiquette_content.booking_initial_contact_title}}{{else}}Initial Contact{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.booking_initial_contact_text}}{{etiquette_content.booking_initial_contact_text}}{{else}}Please introduce yourself politely and include your desired date, time, and duration when reaching out.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#ruby"></use>
                                </svg>
                                {{#if etiquette_content.booking_screening_title}}{{etiquette_content.booking_screening_title}}{{else}}Screening Process{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.booking_screening_text}}{{etiquette_content.booking_screening_text}}{{else}}For everyone's safety, a brief screening process is required for all new clients. This includes basic verification and references.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#diamond"></use>
                                </svg>
                                {{#if etiquette_content.booking_advance_title}}{{etiquette_content.booking_advance_title}}{{else}}Advance Booking{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.booking_advance_text}}{{etiquette_content.booking_advance_text}}{{else}}I prefer bookings made at least 24 hours in advance. Same-day appointments may be available with an additional fee.{{/if}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            {{/if}}
            
            <!-- Respect & Boundaries -->
            {{#if etiquette_content.etiquette_respect_visible}}
            <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-surface), var(--theme-secondary)); position: relative; overflow: hidden;">
                <div style="position: absolute; top: -20px; right: -20px; opacity: 0.2; z-index: 1;">
                    <svg style="width: 80px; height: 80px; color: var(--theme-accent);" viewBox="0 0 64 64">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#crown"></use>
                    </svg>
                </div>
                
                <div style="position: relative; z-index: 2;">
                    <div style="display: flex; align-items: center; gap: 0.8rem; margin-bottom: 2rem; padding-bottom: 1rem; border-bottom: 2px solid var(--theme-accent);">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#scepter"></use>
                        </svg>
                        <h2 style="color: var(--theme-accent); font-size: 1.5rem;">
                            {{#if etiquette_content.respect_title}}{{etiquette_content.respect_title}}{{else}}Respect & Boundaries{{/if}}
                        </h2>
                    </div>
                    
                    <div style="display: grid; gap: 1.5rem;">
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#jewelry"></use>
                                </svg>
                                {{#if etiquette_content.respect_mutual_title}}{{etiquette_content.respect_mutual_title}}{{else}}Mutual Respect{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.respect_mutual_text}}{{etiquette_content.respect_mutual_text}}{{else}}I treat all clients with respect and kindness, and I expect the same in return. Rude or disrespectful behavior will not be tolerated.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#insignia"></use>
                                </svg>
                                {{#if etiquette_content.respect_boundaries_title}}{{etiquette_content.respect_boundaries_title}}{{else}}Professional Boundaries{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.respect_boundaries_text}}{{etiquette_content.respect_boundaries_text}}{{else}}Our time together is for companionship and social interaction. Please respect all boundaries and guidelines.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                                </svg>
                                {{#if etiquette_content.respect_personal_title}}{{etiquette_content.respect_personal_title}}{{else}}Personal Information{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.respect_personal_text}}{{etiquette_content.respect_personal_text}}{{else}}Please do not ask for personal information beyond what is shared publicly. Discretion works both ways.{{/if}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            {{/if}}
            
            <!-- Hygiene & Presentation -->
            {{#if etiquette_content.etiquette_hygiene_visible}}
            <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-surface), var(--theme-primary)); position: relative; overflow: hidden;">
                <div style="position: absolute; top: -20px; right: -20px; opacity: 0.2; z-index: 1;">
                    <svg style="width: 80px; height: 80px; color: var(--theme-accent);" viewBox="0 0 64 64">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#royal"></use>
                    </svg>
                </div>
                
                <div style="position: relative; z-index: 2;">
                    <div style="display: flex; align-items: center; gap: 0.8rem; margin-bottom: 2rem; padding-bottom: 1rem; border-bottom: 2px solid var(--theme-accent);">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#diamond"></use>
                        </svg>
                        <h2 style="color: var(--theme-accent); font-size: 1.5rem;">
                            {{#if etiquette_content.hygiene_title}}{{etiquette_content.hygiene_title}}{{else}}Hygiene & Presentation{{/if}}
                        </h2>
                    </div>
                    
                    <div style="display: grid; gap: 1.5rem;">
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#sapphire"></use>
                                </svg>
                                {{#if etiquette_content.hygiene_personal_title}}{{etiquette_content.hygiene_personal_title}}{{else}}Personal Hygiene{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.hygiene_personal_text}}{{etiquette_content.hygiene_personal_text}}{{else}}Please arrive freshly showered and well-groomed. Good personal hygiene is essential for a comfortable experience.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#ruby"></use>
                                </svg>
                                {{#if etiquette_content.hygiene_attire_title}}{{etiquette_content.hygiene_attire_title}}{{else}}Attire{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.hygiene_attire_text}}{{etiquette_content.hygiene_attire_text}}{{else}}Dress appropriately for our planned activities. I'll always be well-dressed and expect the same consideration.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
                                </svg>
                                {{#if etiquette_content.hygiene_substances_title}}{{etiquette_content.hygiene_substances_title}}{{else}}Substances{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.hygiene_substances_text}}{{etiquette_content.hygiene_substances_text}}{{else}}Please do not arrive under the influence of alcohol or other substances. Light social drinking during our time is acceptable.{{/if}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            {{/if}}
            
            <!-- Cancellation Policy -->
            {{#if etiquette_content.etiquette_cancellation_visible}}
            <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-surface), var(--theme-secondary)); position: relative; overflow: hidden;">
                <div style="position: absolute; top: -20px; right: -20px; opacity: 0.2; z-index: 1;">
                    <svg style="width: 80px; height: 80px; color: var(--theme-accent);" viewBox="0 0 64 64">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#insignia"></use>
                    </svg>
                </div>
                
                <div style="position: relative; z-index: 2;">
                    <div style="display: flex; align-items: center; gap: 0.8rem; margin-bottom: 2rem; padding-bottom: 1rem; border-bottom: 2px solid var(--theme-accent);">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
                        </svg>
                        <h2 style="color: var(--theme-accent); font-size: 1.5rem;">
                            {{#if etiquette_content.cancellation_title}}{{etiquette_content.cancellation_title}}{{else}}Cancellation Policy{{/if}}
                        </h2>
                    </div>
                    
                    <div style="display: grid; gap: 1.5rem;">
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#jewelry"></use>
                                </svg>
                                {{#if etiquette_content.cancellation_advance_title}}{{etiquette_content.cancellation_advance_title}}{{else}}Advance Notice{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.cancellation_advance_text}}{{etiquette_content.cancellation_advance_text}}{{else}}Please provide at least 2 hours notice for cancellations. Last-minute cancellations may result in a cancellation fee.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                                </svg>
                                {{#if etiquette_content.cancellation_noshow_title}}{{etiquette_content.cancellation_noshow_title}}{{else}}No-Shows{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.cancellation_noshow_text}}{{etiquette_content.cancellation_noshow_text}}{{else}}No-shows without notice will result in being blacklisted from future bookings.{{/if}}
                            </p>
                        </div>
                        
                        <div>
                            <h3 style="color: var(--theme-text); font-size: 1.1rem; margin-bottom: 0.5rem; display: flex; align-items: center; gap: 0.5rem;">
                                <svg style="width: 1rem; height: 1rem; color: var(--theme-accent);" viewBox="0 0 64 64">
                                    <use href="/assets/sprites/royal-icons.ddf91c.svg#royal"></use>
                                </svg>
                                {{#if etiquette_content.cancellation_my_title}}{{etiquette_content.cancellation_my_title}}{{else}}My Cancellations{{/if}}
                            </h3>
                            <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                                {{#if etiquette_content.cancellation_my_text}}{{etiquette_content.cancellation_my_text}}{{else}}In the rare event I need to cancel, I'll provide as much notice as possible and offer to reschedule.{{/if}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            {{/if}}
        </div>

        <!-- Safety & Discretion (Full Width) -->
        {{#if etiquette_content.etiquette_safety_visible}}
        <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-primary), var(--theme-secondary)); position: relative; overflow: hidden; margin-bottom: 3rem;">
            <div style="position: absolute; top: -50px; right: -50px; opacity: 0.1; z-index: 1;">
                <svg style="width: 200px; height: 200px; color: var(--theme-accent);" viewBox="0 0 64 64">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
                </svg>
            </div>
            
            <div style="position: relative; z-index: 2; text-align: center;">
                <h2 style="color: var(--theme-text); margin-bottom: 3rem; display: flex; align-items: center; justify-content: center; gap: 0.8rem; font-size: 1.8rem;">
                    <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#insignia"></use>
                    </svg>
                    {{#if etiquette_content.safety_title}}{{etiquette_content.safety_title}}{{else}}Safety & Discretion{{/if}}
                </h2>
                
                <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 2rem;">
                    <div style="text-align: center; padding: 1.5rem;">
                        <div style="margin-bottom: 1.5rem;">
                            <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
                            </svg>
                        </div>
                        <h3 style="color: var(--theme-text); font-size: 1.2rem; margin-bottom: 1rem;">
                            {{#if etiquette_content.safety_confidentiality_title}}{{etiquette_content.safety_confidentiality_title}}{{else}}Confidentiality{{/if}}
                        </h3>
                        <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                            {{#if etiquette_content.safety_confidentiality_text}}{{etiquette_content.safety_confidentiality_text}}{{else}}Your privacy is paramount. All interactions remain strictly confidential.{{/if}}
                        </p>
                    </div>
                    
                    <div style="text-align: center; padding: 1.5rem;">
                        <div style="margin-bottom: 1.5rem;">
                            <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#scepter"></use>
                            </svg>
                        </div>
                        <h3 style="color: var(--theme-text); font-size: 1.2rem; margin-bottom: 1rem;">
                            {{#if etiquette_content.safety_environment_title}}{{etiquette_content.safety_environment_title}}{{else}}Safe Environment{{/if}}
                        </h3>
                        <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                            {{#if etiquette_content.safety_environment_text}}{{etiquette_content.safety_environment_text}}{{else}}I maintain a safe, clean, and welcoming environment for all meetings.{{/if}}
                        </p>
                    </div>
                    
                    <div style="text-align: center; padding: 1.5rem;">
                        <div style="margin-bottom: 1.5rem;">
                            <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#royal"></use>
                            </svg>
                        </div>
                        <h3 style="color: var(--theme-text); font-size: 1.2rem; margin-bottom: 1rem;">
                            {{#if etiquette_content.safety_communication_title}}{{etiquette_content.safety_communication_title}}{{else}}Professional Communication{{/if}}
                        </h3>
                        <p style="color: var(--theme-text); line-height: 1.6; margin: 0;">
                            {{#if etiquette_content.safety_communication_text}}{{etiquette_content.safety_communication_text}}{{else}}All communications are handled professionally and discreetly.{{/if}}
                        </p>
                    </div>
                </div>
            </div>
        </div>
        {{/if}}

        <!-- Questions/Contact CTA Section -->
        {{#if etiquette_content.etiquette_questions_visible}}
        <div class="royal-card" style="background: linear-gradient(135deg, var(--theme-primary), var(--theme-secondary)); position: relative; overflow: hidden;">
            <div style="position: absolute; top: -30px; right: -30px; opacity: 0.2; z-index: 1;">
                <svg style="width: 120px; height: 120px; color: var(--theme-accent);" viewBox="0 0 64 64">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
                </svg>
            </div>
            
            <div style="position: relative; z-index: 2; text-align: center;">
                <h2 style="color: var(--theme-text); margin-bottom: 2rem; display: flex; align-items: center; justify-content: center; gap: 0.8rem;">
                    <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
