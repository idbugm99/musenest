<!-- Royal Gem Rates Page -->
<!-- Rates Hero Section -->
{{#if rates_content.rates_header_visible}}
<section class="royal-section" style="background: linear-gradient(135deg, var(--theme-bg) 0%, var(--theme-bg-alt) 50%, var(--theme-surface) 100%); position: relative;">
    <!-- Royal Background Decorations -->
    <div style="position: absolute; top: 20%; left: 8%; opacity: 0.4; z-index: 1;">
        <svg class="royal-icon-lg" viewBox="0 0 64 64" style="animation: float 8s ease-in-out infinite; width: 3rem; height: 3rem;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#diamond"></use>
        </svg>
    </div>
    <div style="position: absolute; top: 40%; right: 12%; opacity: 0.3; z-index: 1;">
        <svg class="royal-icon" viewBox="0 0 64 64" style="animation: float 6s ease-in-out infinite; animation-delay: 4s;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#ruby"></use>
        </svg>
    </div>
    <div style="position: absolute; bottom: 30%; left: 85%; opacity: 0.5; z-index: 1;">
        <svg class="royal-icon" viewBox="0 0 64 64" style="animation: float 10s ease-in-out infinite; animation-delay: 2s;">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#sapphire"></use>
        </svg>
    </div>

    <div class="royal-container" style="position: relative; z-index: 2; padding: 4rem 2rem; text-align: center;">
        <div style="margin-bottom: 2rem;">
            <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); animation: pulse 4s ease-in-out infinite;">
                <use href="/assets/sprites/royal-icons.ddf91c.svg#crown-royal"></use>
            </svg>
        </div>
        <h1 style="background: linear-gradient(45deg, var(--theme-accent), var(--theme-text)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; margin-bottom: 1.5rem;">
            {{#if rates_content.page_title}}{{rates_content.page_title}}{{else}}Rates & Investment{{/if}}
        </h1>
        
        <!-- Optional Divider -->
        {{#if rates_content.hero_divider_visible}}
        <div style="margin-bottom: 1.5rem;">
            {{#if (eq rates_content.hero_divider_type 'line')}}
            <div style="width: 6rem; height: 2px; background: var(--theme-accent); margin: 0 auto; opacity: 0.7;"></div>
            {{else}}
            <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); opacity: 0.75;">
                <use href="/assets/sprites/royal-icons.ddf91c.svg#{{#if rates_content.hero_divider_icon}}{{rates_content.hero_divider_icon}}{{else}}gem{{/if}}"></use>
            </svg>
            {{/if}}
        </div>
        {{/if}}
        
        {{#if rates_content.page_subtitle}}
        <p style="font-size: 1.2rem; color: var(--theme-text-subtle); max-width: 700px; margin: 0 auto; line-height: 1.6;">
            {{rates_content.page_subtitle}}
        </p>
        {{else}}
        <p style="font-size: 1.2rem; color: var(--theme-text-subtle); max-width: 700px; margin: 0 auto; line-height: 1.6;">
            Exclusive companionship is an investment in unforgettable experiences. My rates reflect the quality, discretion, and sophistication you deserve.
        </p>
        {{/if}}
    </div>
</section>
{{/if}}

<!-- Royal Sticky Navigation -->
<div id="royal-sticky-nav" class="royal-nav-sticky" style="background: var(--theme-surface); border-bottom: 1px solid var(--theme-border); position: sticky; top: 0; z-index: 40; transition: all 0.3s ease; opacity: 0; transform: translateY(-100%); display: none;">
    <div class="royal-container">
        <nav style="display: flex; justify-content: center; align-items: center; padding: 1rem 0; gap: 2rem; flex-wrap: wrap;">
            {{#if rates_content.rates_table_visible}}
            <a href="#rates-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_rates_label}}{{rates_content.nav_rates_label}}{{else}}Rates{{/if}}
            </a>
            {{/if}}
            {{#if rates.extended.length}}
            <a href="#extended-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_extended_label}}{{rates_content.nav_extended_label}}{{else}}Extended{{/if}}
            </a>
            {{/if}}
            {{#if rates_content.rates_additional_visible}}
            <a href="#services-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_services_label}}{{rates_content.nav_services_label}}{{else}}Services{{/if}}
            </a>
            {{/if}}
            <a href="#policies-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_policies_label}}{{rates_content.nav_policies_label}}{{else}}Policies{{/if}}
            </a>
            {{#if rates_content.rates_payment_visible}}
            <a href="#payment-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_payment_label}}{{rates_content.nav_payment_label}}{{else}}Payment{{/if}}
            </a>
            {{/if}}
            {{#if rates_content.cta_visible}}
            <a href="#contact-section" class="royal-nav-link" style="color: var(--theme-text-subtle); font-weight: 500; transition: all 0.3s ease; padding: 0.5rem 1rem; border-radius: var(--radius-sm); text-decoration: none;">
                {{#if rates_content.nav_contact_label}}{{rates_content.nav_contact_label}}{{else}}Contact{{/if}}
            </a>
            {{/if}}
        </nav>
    </div>
</div>

<!-- Main Rates Section -->
{{#if rates_content.rates_table_visible}}
<section id="rates-section" style="padding: 5rem 0; background: var(--theme-bg-alt);">
    <div class="royal-container">
        {{#if rates_content.table_title}}
        <div style="text-align: center; margin-bottom: 3rem;">
            <h2 style="color: var(--theme-accent); margin-bottom: 1rem; display: flex; align-items: center; justify-content: center; gap: 0.8rem;">
                <svg class="royal-icon" viewBox="0 0 64 64">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#jewelry"></use>
                </svg>
                {{rates_content.table_title}}
            </h2>
            {{#if rates_content.table_description}}
            <p style="color: var(--theme-text-subtle); max-width: 600px; margin: 0 auto; line-height: 1.6;">
                {{rates_content.table_description}}
            </p>
            {{/if}}
        </div>
        {{/if}}

        <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(400px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
            <!-- Incall Rates -->
            {{#if rates_content.rates_incall_visible}}
            <div class="royal-card" style="overflow: hidden;">
                <div style="background: linear-gradient(45deg, var(--theme-primary), var(--theme-accent)); color: var(--theme-bg); padding: 1.5rem; text-align: center;">
                    <h3 style="font-size: 1.5rem; font-weight: 700; margin: 0;">Incall Services</h3>
                </div>
                <div style="padding: 2rem;">
                    {{#if rates.incall.length}}
                        {{#each rates.incall}}
                        <div style="display: flex; justify-content: space-between; align-items: center; padding: 1rem 0; border-bottom: 1px solid var(--theme-border-muted); last-child:border-bottom: none;">
                            <div>
                                {{#if ../rates_content.incall_show_service_name}}
                                <div style="font-weight: 600; color: var(--theme-text); margin-bottom: 0.25rem;">
                                    {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                    {{#if this.highlightBadge}}
                                    <span style="background: var(--theme-accent); color: var(--theme-bg); padding: 0.25rem 0.5rem; border-radius: var(--radius-sm); font-size: 0.75rem; margin-left: 0.5rem; font-weight: 700;">
                                        {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}POPULAR{{/if}}
                                    </span>
                                    {{/if}}
                                </div>
                                {{/if}}
                                {{#if ../rates_content.incall_show_duration}}
                                {{#if this.serviceName}}
                                <div style="color: var(--theme-text-subtle); font-size: 0.9rem;">{{this.duration}}</div>
                                {{/if}}
                                {{/if}}
                            </div>
                            {{#if ../rates_content.incall_show_price}}
                            <div style="font-size: 1.25rem; font-weight: 700; color: var(--theme-accent);">${{this.price}}</div>
                            {{/if}}
                        </div>
                        {{/each}}
                    {{else}}
                        <div style="text-align: center; padding: 2rem; color: var(--theme-text-subtle);">
                            <svg class="royal-icon" viewBox="0 0 64 64" style="margin-bottom: 1rem;">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
                            </svg>
                            <p>Contact for personalized rates</p>
                        </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}

            <!-- Outcall Rates -->
            {{#if rates_content.rates_outcall_visible}}
            <div class="royal-card" style="overflow: hidden;">
                <div style="background: linear-gradient(45deg, var(--theme-secondary), var(--theme-primary)); color: var(--theme-text); padding: 1.5rem; text-align: center;">
                    <h3 style="font-size: 1.5rem; font-weight: 700; margin: 0;">Outcall Services</h3>
                </div>
                <div style="padding: 2rem;">
                    {{#if rates.outcall.length}}
                        {{#each rates.outcall}}
                        <div style="display: flex; justify-content: space-between; align-items: center; padding: 1rem 0; border-bottom: 1px solid var(--theme-border-muted); last-child:border-bottom: none;">
                            <div>
                                {{#if ../rates_content.outcall_show_service_name}}
                                <div style="font-weight: 600; color: var(--theme-text); margin-bottom: 0.25rem;">
                                    {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                    {{#if this.highlightBadge}}
                                    <span style="background: var(--theme-accent); color: var(--theme-bg); padding: 0.25rem 0.5rem; border-radius: var(--radius-sm); font-size: 0.75rem; margin-left: 0.5rem; font-weight: 700;">
                                        {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}POPULAR{{/if}}
                                    </span>
                                    {{/if}}
                                </div>
                                {{/if}}
                                {{#if ../rates_content.outcall_show_duration}}
                                {{#if this.serviceName}}
                                <div style="color: var(--theme-text-subtle); font-size: 0.9rem;">{{this.duration}}</div>
                                {{/if}}
                                {{/if}}
                            </div>
                            {{#if ../rates_content.outcall_show_price}}
                            <div style="font-size: 1.25rem; font-weight: 700; color: var(--theme-secondary);">${{this.price}}</div>
                            {{/if}}
                        </div>
                        {{/each}}
                    {{else}}
                        <div style="text-align: center; padding: 2rem; color: var(--theme-text-subtle);">
                            <svg class="royal-icon" viewBox="0 0 64 64" style="margin-bottom: 1rem;">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
                            </svg>
                            <p>Contact for personalized rates</p>
                        </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
        </div>
    </div>
</section>
{{/if}}

<!-- Extended Engagements Section -->
{{#if rates_content.rates_extended_visible}}
{{#if rates.extended.length}}
<section id="extended-section" style="padding: 5rem 0; background: var(--theme-surface);">
    <div class="royal-container">
        <div style="text-align: center; margin-bottom: 3rem;">
            <h2 style="color: var(--theme-accent); margin-bottom: 1rem; display: flex; align-items: center; justify-content: center; gap: 0.8rem;">
                <svg class="royal-icon" viewBox="0 0 64 64">
                    <use href="/assets/sprites/royal-icons.ddf91c.svg#crown"></use>
                </svg>
                Extended Experiences
            </h2>
        </div>
        
        <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem;">
            {{#each rates.extended}}
            <div class="royal-card" style="text-align: center; position: relative; overflow: hidden; transition: transform 0.3s ease;" onmouseover="this.style.transform='translateY(-5px)'" onmouseout="this.style.transform='translateY(0)'">
                <div style="position: absolute; top: -20px; right: -20px; opacity: 0.1; z-index: 1;">
                    <svg style="width: 80px; height: 80px; color: var(--theme-accent);" viewBox="0 0 64 64">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#diamond"></use>
                    </svg>
                </div>
                
                <div style="position: relative; z-index: 2; padding: 2rem;">
                    <div style="margin-bottom: 1.5rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#insignia"></use>
                        </svg>
                    </div>
                    <h3 style="color: var(--theme-text); margin-bottom: 1rem; font-size: 1.25rem;">{{this.serviceName}}</h3>
                    {{#if this.price}}
                    <div style="color: var(--theme-accent); font-size: 1.75rem; font-weight: 700; margin-bottom: 1rem;">${{this.price}}</div>
                    {{else}}
                    <div style="color: var(--theme-accent); font-size: 1.75rem; font-weight: 700; margin-bottom: 1rem;">Custom</div>
                    {{/if}}
                    <p style="color: var(--theme-text-subtle); line-height: 1.6;">{{this.duration}}</p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/if}}
{{/if}}

<!-- Services and Policies Section -->
<section id="services-section" style="padding: 5rem 0; background: var(--theme-bg-alt);">
    <div class="royal-container">
        <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(400px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
            
            <!-- Additional Services -->
            {{#if rates_content.rates_additional_visible}}
            <div class="royal-card">
                <div style="text-align: center; margin-bottom: 2rem;">
                    <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-bottom: 1rem;">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#scepter"></use>
                    </svg>
                    <h3 style="color: var(--theme-text); font-size: 1.5rem;">Additional Services</h3>
                </div>
                
                {{#if additionalServices.length}}
                    {{#each additionalServices}}
                    <div style="display: flex; justify-content: space-between; align-items: center; padding: 0.75rem 0; border-bottom: 1px solid var(--theme-border-muted);">
                        <div>
                            <div style="font-weight: 500; color: var(--theme-text);">{{this.service_name}}</div>
                            {{#if this.description}}
                            <div style="font-size: 0.9rem; color: var(--theme-text-subtle);">{{this.description}}</div>
                            {{/if}}
                        </div>
                        <div style="color: var(--theme-accent); font-weight: 600;">${{this.price}}</div>
                    </div>
                    {{/each}}
                {{else}}
                    <div style="text-align: center; padding: 2rem; color: var(--theme-text-subtle);">
                        <p>Dinner dates: +$100 (restaurant cost separate)</p>
                        <p>Travel outside city: +$500/day + expenses</p>
                    </div>
                {{/if}}
            </div>
            {{/if}}
            
            <!-- Booking Policies -->
            <div id="policies-section" class="royal-card">
                <div style="text-align: center; margin-bottom: 2rem;">
                    <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-bottom: 1rem;">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#jewelry"></use>
                    </svg>
                    <h3 style="color: var(--theme-text); font-size: 1.5rem;">Booking Policies</h3>
                </div>
                
                <div style="color: var(--theme-text-subtle); line-height: 1.7;">
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>Advance booking recommended for optimal scheduling</span>
                    </div>
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>Same day booking: additional $100 fee</span>
                    </div>
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>Screening required for all new clients</span>
                    </div>
                </div>
            </div>
        </div>
        
        <!-- Payment Terms and Terms Grid -->
        <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(400px, 1fr)); gap: 2rem;">
            <!-- Payment Terms -->
            {{#if rates_content.rates_payment_visible}}
            <div id="payment-section" class="royal-card">
                <div style="text-align: center; margin-bottom: 2rem;">
                    <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-bottom: 1rem;">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#sapphire"></use>
                    </svg>
                    <h3 style="color: var(--theme-text); font-size: 1.5rem;">Payment Information</h3>
                </div>
                
                <div style="color: var(--theme-text-subtle); line-height: 1.7;">
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>Cash, bank transfer, or cryptocurrency accepted</span>
                    </div>
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>Payment required at the beginning of our time together</span>
                    </div>
                    <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                        <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                            <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                        </svg>
                        <span>All transactions are confidential and discrete</span>
                    </div>
                </div>
            </div>
            {{/if}}
            
            <!-- Terms & Additional Information -->
            {{#if rates_content.terms_visible}}
            <div class="royal-card">
                <div style="text-align: center; margin-bottom: 2rem;">
                    <svg class="royal-icon-lg" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-bottom: 1rem;">
                        <use href="/assets/sprites/royal-icons.ddf91c.svg#ruby"></use>
                    </svg>
                    <h3 style="color: var(--theme-text); font-size: 1.5rem;">
                        {{#if rates_content.terms_title}}{{rates_content.terms_title}}{{else}}Terms & Conditions{{/if}}
                    </h3>
                </div>
                <div style="color: var(--theme-text-subtle); line-height: 1.7;">
                    {{#if rates_content.terms_content}}
                        {{#each (split rates_content.terms_content " • ")}}
                        <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                            <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                            </svg>
                            <span>{{this}}</span>
                        </div>
                        {{/each}}
                    {{else}}
                        <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                            <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                            </svg>
                            <span>Cancellation with less than 2 hours notice may incur a fee</span>
                        </div>
                        <div style="margin-bottom: 1rem; display: flex; align-items: start; gap: 0.75rem;">
                            <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent); margin-top: 0.25rem; flex-shrink: 0;">
                                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem-alt"></use>
                            </svg>
                            <span>Extended bookings require a 25% deposit</span>
                        </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
        </div>
    </div>
</section>

<!-- Donations Section -->
{{#if rates_content.donations_visible}}
<section style="padding: 3rem 0; background: var(--theme-surface); text-align: center;">
    <div class="royal-container">
        <div style="margin-bottom: 1rem;">
            <svg class="royal-icon" viewBox="0 0 64 64" style="color: var(--theme-accent);">
                <use href="/assets/sprites/royal-icons.ddf91c.svg#gem"></use>
            </svg>
        </div>
        <h2 style="color: var(--theme-accent); margin-bottom: 1rem;">
            {{#if rates_content.donation_title}}{{rates_content.donation_title}}{{else}}Donations{{/if}}
        </h2>
        <p style="color: var(--theme-text-subtle); max-width: 600px; margin: 0 auto; line-height: 1.6;">
            {{#if rates_content.donation_description}}{{rates_content.donation_description}}{{else}}Donations are appreciated to help support my work{{/if}}
        </p>
    </div>
</section>
{{/if}}

<!-- Contact CTA Section -->
{{#if rates_content.cta_visible}}
<section id="contact-section" class="royal-card" style="background: linear-gradient(135deg, var(--theme-primary), var(--theme-secondary)); position: relative; overflow: hidden; margin: 4rem 2rem;">
    <div style="position: absolute; top: -30px; right: -30px; opacity: 0.2; z-index: 1;">
        <svg style="width: 120px; height: 120px; color: var(--theme-accent);" viewBox="0 0 64 64">
            <use href="/assets/sprites/royal-icons.ddf91c.svg#royal"></use>
        </svg>
    </div>
    
    <div style="position: relative; z-index: 2; text-align: center; padding: 4rem 2rem;">
        <h2 style="color: var(--theme-text); margin-bottom: 1.5rem; display: flex; align-items: center; justify-content: center; gap: 0.8rem;">
            <svg class="royal-icon" viewBox="0 0 64 64">
                <use href="/assets/sprites/royal-icons.ddf91c.svg#crown"></use>
            </svg>
