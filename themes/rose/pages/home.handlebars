                <div class="rose-service-bloom">🌹</div>
                <div class="rose-service-content">
                    <div class="rose-service-icon">
                        {{#if content.service_1_icon}}{{content.service_1_icon}}{{else}}💎{{/if}}
                    </div>
                    <h3 class="rose-service-title">
                        {{#if content.service_1_title}}{{content.service_1_title}}{{else}}Premium Experience{{/if}}
                    </h3>
                    {{#if content.service_1_description}}
                    <p class="rose-service-description">{{content.service_1_description}}</p>
                    {{/if}}
                </div>
            </div>
            {{/if}}
            
            {{#if content.service_2_visible}}
            <div class="rose-service-card" data-aos="fade-up" data-aos-delay="200">
                <div class="rose-service-bloom">🌹</div>
                <div class="rose-service-content">
                    <div class="rose-service-icon">
                        {{#if content.service_2_icon}}{{content.service_2_icon}}{{else}}👑{{/if}}
                    </div>
                    <h3 class="rose-service-title">
                        {{#if content.service_2_title}}{{content.service_2_title}}{{else}}Elite Companionship{{/if}}
