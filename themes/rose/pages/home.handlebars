<!-- Rose Garden Home Page - Romantic Rose-Inspired Design -->

<!-- Rose Garden Hero Section -->
{{#if content.hero_section_visible}}
<section class="rose-hero" data-aos="fade-in">
    <!-- Floating Rose Petals -->
    <div class="rose-petals-container">
        <div class="rose-petal rose-petal-1"></div>
        <div class="rose-petal rose-petal-2"></div>
        <div class="rose-petal rose-petal-3"></div>
        <div class="rose-petal rose-petal-4"></div>
        <div class="rose-petal rose-petal-5"></div>
        <div class="rose-petal rose-petal-6"></div>
    </div>
    
    <!-- Rose Garden Background Pattern -->
    <div class="rose-garden-pattern"></div>
    
    <!-- Hero Content -->
    <div class="rose-hero-content">
        <div class="max-w-5xl mx-auto text-center px-4 sm:px-6 lg:px-8">
            <!-- Rose Icon -->
            <div class="rose-icon" data-aos="fade-down" data-aos-delay="100">
                🌹
            </div>
            
            <!-- Elegant Title -->
            <h1 class="rose-hero-title" data-aos="fade-up" data-aos-delay="300">
                {{#if content.hero_title}}{{content.hero_title}}{{else}}{{siteName}}{{/if}}
            </h1>
            
            <!-- Romantic Subtitle -->
            {{#if content.hero_subtitle}}
            <p class="rose-hero-subtitle" data-aos="fade-up" data-aos-delay="500">
                {{content.hero_subtitle}}
            </p>
            {{/if}}
            
            <!-- Rose CTA Buttons -->
            <div class="rose-cta-container" data-aos="fade-up" data-aos-delay="700">
                {{#if content.hero_button_1_text}}
                    {{#if (eq content.hero_button_1_link 'calendar')}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{else if (eq content.hero_button_1_link 'contact')}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{else if (eq content.hero_button_1_link 'about')}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{else if (eq content.hero_button_1_link 'rates')}}
                        <a href="/{{modelSlug}}/rates{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{else if (eq content.hero_button_1_link 'gallery')}}
                        <a href="/{{modelSlug}}/gallery{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{else}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="rose-btn rose-btn-primary">
                    {{/if}}
                        <span>{{content.hero_button_1_text}}</span>
                    </a>
                {{else}}
                    <a href="/{{modelSlug}}/contact{{previewParam}}" class="rose-btn rose-btn-primary">
                        <span>Contact Me</span>
                    </a>
                {{/if}}

                {{#if content.hero_button_2_text}}
                    {{#if (eq content.hero_button_2_link 'calendar')}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{else if (eq content.hero_button_2_link 'contact')}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{else if (eq content.hero_button_2_link 'about')}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{else if (eq content.hero_button_2_link 'rates')}}
                        <a href="/{{modelSlug}}/rates{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{else if (eq content.hero_button_2_link 'gallery')}}
                        <a href="/{{modelSlug}}/gallery{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{else}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="rose-btn rose-btn-secondary">
                    {{/if}}
                        <span>{{content.hero_button_2_text}}</span>
                    </a>
                {{else}}
                    <a href="/{{modelSlug}}/gallery{{previewParam}}" class="rose-btn rose-btn-secondary">
                        <span>View Gallery</span>
                    </a>
                {{/if}}
            </div>
        </div>
    </div>
</section>
{{/if}}

<!-- Rose About Section -->
{{#if content.about_section_visible}}
<section class="rose-about-section" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid lg:grid-cols-2 gap-16 items-center">
            
            <!-- Portrait with Rose Frame -->
            {{#if content.portrait_section_visible}}
            <div class="rose-portrait-garden" data-aos="fade-right">
                <div class="rose-frame">
                    {{#if content.portrait_image_url}}
                    <img src="{{content.portrait_image_url}}" 
                         alt="{{#if content.portrait_alt}}{{content.portrait_alt}}{{else}}Rose Portrait{{/if}}"
                         class="rose-portrait-image">
                    {{else}}
                    <div class="rose-portrait-placeholder">
                        <div class="rose-placeholder-icon">🌹</div>
                        <p>Rose Portrait</p>
                    </div>
                    {{/if}}
                    
                    <!-- Rose Border Effects -->
                    <div class="rose-border-roses">
                        <div class="rose-corner rose-corner-tl">🌹</div>
                        <div class="rose-corner rose-corner-tr">🌹</div>
                        <div class="rose-corner rose-corner-bl">🌹</div>
                        <div class="rose-corner rose-corner-br">🌹</div>
                    </div>
                </div>
                
                <!-- Rose Glow Effect -->
                <div class="rose-glow"></div>
            </div>
            {{/if}}
            
            <!-- About Content with Rose Typography -->
            <div class="rose-about-content" data-aos="fade-left">
                <div class="rose-section-header">
                    <div class="rose-divider">
                        <span class="rose-divider-rose">🌹</span>
                    </div>
                    <h2 class="rose-section-title">
                        {{#if content.about_title}}{{content.about_title}}{{else}}About {{siteName}}{{/if}}
                    </h2>
                    <div class="rose-divider">
                        <span class="rose-divider-rose">🌹</span>
                    </div>
                </div>
                
                {{#if content.about_paragraph_1}}
                <div class="rose-text-content">
                    <p class="rose-lead-text">{{content.about_paragraph_1}}</p>
                </div>
                {{/if}}
                {{#if content.about_paragraph_2}}
                <div class="rose-text-content">
                    <p class="rose-lead-text">{{content.about_paragraph_2}}</p>
                </div>
                {{/if}}
                
                {{#if content.about_link_text}}
                <div class="rose-about-cta">
                    {{#eq content.about_link_destination 'about'}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="rose-link">
                    {{/eq}}
                    {{#eq content.about_link_destination 'contact'}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="rose-link">
                    {{/eq}}
                    {{#eq content.about_link_destination 'gallery'}}
                        <a href="/{{modelSlug}}/gallery{{previewParam}}" class="rose-link">
                    {{/eq}}
                    {{#eq content.about_link_destination 'rates'}}
                        <a href="/{{modelSlug}}/rates{{previewParam}}" class="rose-link">
                    {{/eq}}
                    {{#eq content.about_link_destination 'calendar'}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="rose-link">
                    {{/eq}}
                        {{content.about_link_text}} <span class="rose-arrow">→</span>
                    </a>
                </div>
                {{else}}
                <div class="rose-about-cta">
                    <a href="/{{modelSlug}}/about{{previewParam}}" class="rose-link">Learn More <span class="rose-arrow">→</span></a>
                </div>
                {{/if}}
            </div>
        </div>
    </div>
</section>
{{/if}}

<!-- Rose Gallery Section -->
{{#if content.gallery_section_visible}}
<section class="rose-gallery-section" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="rose-section-header text-center">
            <h2 class="rose-section-title">
                {{#if content.gallery_section_title}}{{content.gallery_section_title}}{{else}}Rose Gallery{{/if}}
            </h2>
            <div class="rose-ornate-separator">
                <span class="rose-separator-bloom">🌹</span>
                <span class="rose-separator-line"></span>
                <span class="rose-separator-bloom">🌹</span>
            </div>
        </div>

        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-6">
            {{#each galleryImages}}
            <div class="rose-gallery-item" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <img src="/uploads/{{../modelSlug}}/public/gallery/{{this.filename}}" 
                     alt="{{#if this.caption}}{{this.caption}}{{else}}Rose Gallery{{/if}}" 
                     class="rose-gallery-image" loading="lazy">
                <div class="rose-image-overlay">
                    <div class="rose-overlay-icon">🌹</div>
                </div>
            </div>
            {{else}}
            {{#times 5}}
            <div class="rose-gallery-placeholder" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <div class="rose-placeholder-icon">🌹</div>
                <p>Rose Gallery</p>
            </div>
            {{/times}}
            {{/each}}
        </div>

        <div class="text-center mt-12">
            <a href="/{{modelSlug}}/gallery{{previewParam}}" class="rose-btn rose-btn-garden">
                <span>{{#if content.gallery_button_text}}{{content.gallery_button_text}}{{else}}View Rose Garden{{/if}}</span>
            </a>
        </div>
    </div>
</section>
{{/if}}

<!-- Rose Travel Section -->
{{#if content.travel_section_visible}}
<section class="rose-travel-section" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="rose-section-header text-center">
            <h2 class="rose-section-title">
                {{#if content.travel_section_title}}{{content.travel_section_title}}{{else}}Rose Garden Tour{{/if}}
            </h2>
            <div class="rose-ornate-separator">
                <span class="rose-separator-bloom">🌹</span>
                <span class="rose-separator-line"></span>
                <span class="rose-separator-bloom">🌹</span>
            </div>
        </div>
        
        {{#if upcomingEvents}}
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
            {{#each upcomingEvents}}
            <div class="rose-travel-card" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <div class="rose-card-header">
                    <div class="rose-location-icon">📍</div>
                    <h3 class="rose-card-title">{{this.location}}</h3>
                    <div class="rose-date-badge">
                        {{#if this.dateRange}}{{this.dateRange}}{{else}}{{formatDate this.date}}{{/if}}
                    </div>
                </div>
                <div class="rose-card-content">
                    <span class="rose-available-badge">Available</span>
                    {{#if this.notes}}
                    <p class="rose-travel-notes">{{this.notes}}</p>
                    {{/if}}
                </div>
            </div>
            {{/each}}
        </div>
        
        <div class="text-center mt-12">
            <a href="/{{modelSlug}}/{{#if content.travel_cta_link}}{{content.travel_cta_link}}{{else}}calendar{{/if}}{{previewParam}}" 
               class="rose-btn rose-btn-garden">
                <span>{{#if content.travel_cta_text}}{{content.travel_cta_text}}{{else}}View Full Calendar{{/if}}</span>
            </a>
        </div>
        {{else}}
        <div class="text-center">
            <div class="rose-travel-card max-w-md mx-auto">
                <div class="rose-card-header">
                    <div class="rose-location-icon">📅</div>
                    <h3 class="rose-card-title">Check My Calendar</h3>
                    <p class="rose-travel-notes">View available dates and book your appointment</p>
                </div>
                <div class="rose-card-content">
                    <a href="/{{modelSlug}}/calendar{{previewParam}}" class="rose-btn rose-btn-primary">
                        <span>View Calendar</span>
                    </a>
                </div>
            </div>
        </div>
        {{/if}}
    </div>
</section>
{{/if}}

<!-- Rose Testimonials Section -->
{{#if content.testimonials_section_visible}}
<section class="rose-testimonials-section" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="rose-section-header text-center">
            <h2 class="rose-section-title">
                {{#if content.testimonials_section_title}}{{content.testimonials_section_title}}{{else}}Rose Garden Reviews{{/if}}
            </h2>
            <div class="rose-ornate-separator">
                <span class="rose-separator-bloom">🌹</span>
                <span class="rose-separator-line"></span>
                <span class="rose-separator-bloom">🌹</span>
            </div>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
            {{#each testimonials}}
            {{#if (lt @index (or content.testimonials_display_count 3))}}
            <div class="rose-testimonial-card" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <div class="rose-stars">
                    {{#times this.rating}}🌹{{/times}}
                </div>
                <blockquote class="rose-testimonial-text">"{{this.text}}"</blockquote>
                <cite class="rose-testimonial-author">{{this.name}}</cite>
            </div>
            {{/if}}
            {{else}}
            <div class="rose-testimonial-card">
                <div class="rose-stars">
                    🌹🌹🌹🌹🌹
                </div>
                <blockquote class="rose-testimonial-text">"Elegant, sophisticated, and truly enchanting. An absolutely magical experience."</blockquote>
                <cite class="rose-testimonial-author">Michael D.</cite>
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/if}}

<!-- Rose Services Section -->
