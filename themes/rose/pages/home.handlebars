            {{#if content.service_3_visible}}
            <div class="rose-service-card" data-aos="fade-up" data-aos-delay="300">
                <div class="rose-service-bloom">🌹</div>
                <div class="rose-service-content">
                    <div class="rose-service-icon">
                        {{#if content.service_3_icon}}{{content.service_3_icon}}{{else}}✨{{/if}}
                    </div>
                    <h3 class="rose-service-title">
                        {{#if content.service_3_title}}{{content.service_3_title}}{{else}}Exclusive Access{{/if}}
                    </h3>
                    {{#if content.service_3_description}}
                    <p class="rose-service-description">{{content.service_3_description}}</p>
                    {{/if}}
                </div>
            </div>
            {{/if}}
        </div>
        
        {{#if content.services_cta_text}}
        <div class="text-center mt-16" data-aos="fade-up" data-aos-delay="400">
            <a href="{{#if content.services_cta_link}}{{content.services_cta_link}}{{else}}/{{modelSlug}}/rates{{/if}}" 
               class="rose-btn rose-btn-garden">
                <span>{{#if content.services_cta_text}}{{content.services_cta_text}}{{else}}View Rose Services{{/if}}</span>
            </a>
        </div>
        {{/if}}
    </div>
</section>
