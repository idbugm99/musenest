<!-- Rose Garden Etiquette Page Styles -->
<style>
/* Rose Garden Etiquette Color Palette */
:root {
    --rose-deep: #b91c1c; /* Deep rose red */
    --rose-primary: #dc2626; /* Vibrant rose */
    --rose-light: #fecaca; /* Soft blush pink */
    --rose-blush: #fef2f2; /* Lightest rose tint */
    --rose-white: #fefefe; /* Pure white */
    --rose-gold: #d4af37; /* Elegant gold accent */
    --rose-bronze: #cd7f32; /* Warm bronze */
    --rose-text: #881337; /* Deep rose text */
    --rose-muted: #9f1239; /* Muted rose */
    --rose-shadow: rgba(185, 28, 28, 0.15); /* Soft rose shadow */
    --rose-gradient: linear-gradient(135deg, #fef2f2 0%, #fecaca 50%, #fbb6ce 100%);
    --rose-dark-gradient: linear-gradient(135deg, #881337 0%, #be185d 50%, #ec4899 100%);
}

/* Etiquette Header Styling */
.rose-etiquette-header {
    position: relative;
    padding: 8rem 0 6rem;
    background: linear-gradient(135deg, var(--rose-text) 0%, var(--rose-muted) 50%, var(--rose-deep) 100%);
    overflow: hidden;
    min-height: 65vh;
}

.rose-etiquette-garden-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: 
        radial-gradient(ellipse at 25% 25%, rgba(220, 38, 38, 0.08) 0%, transparent 50%),
        radial-gradient(ellipse at 75% 75%, rgba(254, 202, 202, 0.08) 0%, transparent 50%),
        radial-gradient(ellipse at 50% 50%, rgba(220, 38, 38, 0.05) 0%, transparent 70%);
    filter: blur(40px);
}

.rose-etiquette-decorations {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
}

.rose-floating-element {
    position: absolute;
    font-size: 2.5rem;
    opacity: 0.4;
    animation: roseEtiquetteFloat 20s infinite ease-in-out;
}

.rose-element-1 { top: 15%; left: 10%; animation-delay: 0s; }
.rose-element-2 { top: 25%; right: 15%; animation-delay: 3s; }
.rose-element-3 { top: 45%; left: 20%; animation-delay: 6s; }
.rose-element-4 { bottom: 30%; right: 10%; animation-delay: 9s; }
.rose-element-5 { bottom: 15%; left: 15%; animation-delay: 12s; }
.rose-element-6 { top: 60%; right: 25%; animation-delay: 15s; }

@keyframes roseEtiquetteFloat {
    0%, 100% { transform: translateY(0px) rotate(0deg) scale(1); opacity: 0.4; }
    33% { transform: translateY(-25px) rotate(120deg) scale(1.1); opacity: 0.6; }
    66% { transform: translateY(-10px) rotate(240deg) scale(0.9); opacity: 0.5; }
}

.rose-garden-protocol-emblem {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 3rem;
}

.rose-emblem-center {
    font-size: 4.5rem;
    color: var(--rose-white);
    filter: drop-shadow(0 0 30px rgba(254, 254, 254, 0.5));
    animation: roseEmblemPulse 6s infinite ease-in-out;
}

.rose-emblem-petals {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.rose-emblem-petal {
    position: absolute;
    font-size: 1.8rem;
    color: var(--rose-white);
    animation: roseEmblemPetalsOrbit 20s infinite linear;
    filter: drop-shadow(0 0 15px rgba(254, 254, 254, 0.3));
}

.rose-emblem-petal-1 { top: -80px; left: 50%; transform: translateX(-50%); animation-delay: 0s; }
.rose-emblem-petal-2 { top: -40px; right: -40px; animation-delay: 4s; }
.rose-emblem-petal-3 { top: 50%; right: -80px; transform: translateY(-50%); animation-delay: 8s; }
.rose-emblem-petal-4 { bottom: -40px; right: -40px; animation-delay: 12s; }
.rose-emblem-petal-5 { top: 50%; left: -80px; transform: translateY(-50%); animation-delay: 16s; }

@keyframes roseEmblemPulse {
    0%, 100% { transform: scale(1); filter: drop-shadow(0 0 30px rgba(254, 254, 254, 0.5)); }
    50% { transform: scale(1.1); filter: drop-shadow(0 0 40px rgba(254, 254, 254, 0.7)); }
}

@keyframes roseEmblemPetalsOrbit {
    0% { transform: translate(-50%, -50%) rotate(0deg) scale(0.8); opacity: 0.6; }
    50% { transform: translate(-50%, -50%) rotate(180deg) scale(1.2); opacity: 1; }
    100% { transform: translate(-50%, -50%) rotate(360deg) scale(0.8); opacity: 0.6; }
}

.rose-etiquette-title {
    font-size: 4.5rem;
    font-weight: 900;
    color: var(--rose-white);
    text-align: center;
    margin-bottom: 2rem;
    text-shadow: 0 4px 20px rgba(0, 0, 0, 0.5);
    letter-spacing: 2px;
    font-family: 'Playfair Display', serif;
}

.rose-etiquette-subtitle {
    font-size: 1.4rem;
    color: var(--rose-white);
    text-align: center;
    opacity: 0.9;
    font-style: italic;
    max-width: 800px;
    margin: 0 auto;
    line-height: 1.7;
    text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);
}

/* Protocol Gardens Section */
.rose-protocol-gardens {
    padding: 8rem 0;
    background: var(--rose-gradient);
    position: relative;
}

/* Garden Pavilion Styling */
.rose-booking-garden,
.rose-respect-garden,
.rose-hygiene-garden,
.rose-communication-garden {
    position: relative;
    background: linear-gradient(145deg, rgba(255, 255, 255, 0.95) 0%, rgba(254, 242, 242, 0.95) 100%);
    border-radius: 25px;
    padding: 2rem;
    box-shadow: 0 20px 40px var(--rose-shadow);
