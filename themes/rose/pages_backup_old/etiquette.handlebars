<!-- Rose Garden Etiquette JavaScript -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    // Enhanced rose garden animations
    const roseElements = document.querySelectorAll('.rose-floating-element');
    roseElements.forEach((element, index) => {
        element.style.animationDelay = `${index * 2}s`;
        
        // Add hover effect
        element.addEventListener('mouseenter', function() {
            this.style.animationDuration = '2s';
            this.style.transform = 'scale(1.5)';
        });
        
        element.addEventListener('mouseleave', function() {
            this.style.animationDuration = '20s';
            this.style.transform = 'scale(1)';
        });
    });
    
    // Rose garden item bloom effects
    const gardenItems = document.querySelectorAll('.rose-garden-item');
    gardenItems.forEach(item => {
        item.addEventListener('mouseenter', function() {
            const bloom = this.querySelector('.rose-item-bloom');
            const glow = this.querySelector('.rose-item-glow');
            
            if (bloom) {
                bloom.style.transform = 'scale(1.2) rotate(15deg)';
                bloom.style.transition = 'all 0.3s ease';
            }
            
            if (glow) {
                glow.style.animationDuration = '1s';
            }
        });
        
        item.addEventListener('mouseleave', function() {
            const bloom = this.querySelector('.rose-item-bloom');
            const glow = this.querySelector('.rose-item-glow');
            
            if (bloom) {
                bloom.style.transform = 'scale(1) rotate(0deg)';
            }
            
            if (glow) {
                glow.style.animationDuration = '3s';
            }
        });
    });
    
    // Rose garden arch bloom sequence
    const archBlooms = document.querySelectorAll('.rose-arch-bloom');
    let bloomIndex = 0;
    
    setInterval(() => {
        archBlooms.forEach((bloom, index) => {
            bloom.style.transform = index === bloomIndex 
                ? 'scale(1.5) rotate(180deg)' 
                : 'scale(1) rotate(0deg)';
        });
        
        bloomIndex = (bloomIndex + 1) % archBlooms.length;
    }, 2000);
    
    // Rose emblem petal interaction
    const emblemCenter = document.querySelector('.rose-emblem-center');
    const emblemPetals = document.querySelectorAll('.rose-emblem-petal');
    
    if (emblemCenter) {
        emblemCenter.addEventListener('mouseenter', function() {
            emblemPetals.forEach(petal => {
                petal.style.animationDuration = '5s';
                petal.style.transform = 'translate(-50%, -50%) scale(1.3)';
            });
        });
        
        emblemCenter.addEventListener('mouseleave', function() {
            emblemPetals.forEach(petal => {
                petal.style.animationDuration = '20s';
                petal.style.transform = 'translate(-50%, -50%) scale(1)';
            });
        });
    }
    
    // Scroll-triggered rose blooming effects
    const observerOptions = {
        threshold: 0.1,
        rootMargin: '0px 0px -50px 0px'
    };
    
    const gardenObserver = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                const archBlooms = entry.target.querySelectorAll('.rose-arch-bloom');
                archBlooms.forEach((bloom, index) => {
                    setTimeout(() => {
                        bloom.style.transform = 'scale(1.5) rotate(360deg)';
                        setTimeout(() => {
                            bloom.style.transform = 'scale(1) rotate(0deg)';
                        }, 600);
                    }, index * 200);
                });
            }
        });
    }, observerOptions);
    
    // Observe all garden pavilions
    const gardenPavilions = document.querySelectorAll('.rose-booking-garden, .rose-respect-garden, .rose-hygiene-garden, .rose-communication-garden');
    gardenPavilions.forEach(pavilion => {
        gardenObserver.observe(pavilion);
    });
    
    // Add sparkle effect on scroll
    let sparkleTimer;
    window.addEventListener('scroll', function() {
        clearTimeout(sparkleTimer);
        sparkleTimer = setTimeout(() => {
            const sparkleElements = document.querySelectorAll('.rose-item-glow');
            sparkleElements.forEach(element => {
                element.style.animationDuration = '1s';
                setTimeout(() => {
                    element.style.animationDuration = '3s';
                }, 1000);
            });
        }, 100);
    });
});
</script>