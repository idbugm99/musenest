{
  "theme_name": "Rose Theme",
  "theme_version": "2.0.0",
  "theme_type": "enhanced",
  "extends": "universal",
  
  "custom_elements": {
    "decorative_flowers": {
      "enabled": true,
      "type": "rose",
      "animation": "bloom",
      "count": 3,
      "positions": ["top-right", "bottom-left", "center-left"]
    },
    "floating_petals": {
      "enabled": true,
      "count": 4,
      "animation": "float",
      "duration": "8s"
    },
    "vine_borders": {
      "enabled": true,
      "style": "elegant",
      "emoji": "ðŸŒ¹"
    },
    "garden_background": {
      "enabled": true,
      "pattern": "radial-dots",
      "opacity": "subtle",
      "animation": "drift"
    }
  },
  
  "enhanced_components": {
    "buttons": {
      "style": "rose-gradient",
      "shimmer_effect": true,
      "shadow_color": "accent-based"
    },
    "cards": {
      "border_glow": true,
      "background_tint": "2%",
      "hover_border": "gradient"
    },
    "section_headers": {
      "style": "rose-line",
      "emoji": "ðŸŒ¹",
      "underline": "gradient"
    }
  },
  
  "theme_variations": {
    "subtle": {
      "decorative_opacity": 0.6,
      "animation_speed": "slow",
      "flower_size": "small"
    },
    "elegant": {
      "decorative_opacity": 0.8,
      "animation_speed": "medium",
      "flower_size": "medium"
    },
    "dramatic": {
      "decorative_opacity": 1.0,
      "animation_speed": "fast",
      "flower_size": "large"
    }
  },
  
  "responsive_settings": {
    "mobile": {
      "reduce_decorations": true,
      "flower_size": "small",
      "disable_animations": false
    },
    "tablet": {
      "reduce_decorations": false,
      "flower_size": "medium",
      "disable_animations": false
    }
  }
}