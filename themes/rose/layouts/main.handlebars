<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{pageTitle}} - {{siteName}}</title>
    <meta name="description" content="{{metaDescription}}">
    <meta name="og:title" content="{{pageTitle}} - {{siteName}}">
    <meta name="og:description" content="{{metaDescription}}">
    <meta name="og:type" content="website">
    {{#if ogImage}}
    <meta name="og:image" content="{{ogImage}}">
    {{/if}}
    
    <!-- Tailwind CSS -->
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
    
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <!-- Google Fonts - Luxury Typography -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@400;500;600;700;800;900&family=Poppins:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    
    <!-- AOS Animation Library -->
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    
    <!-- Rose Icons Sprite Preload -->
    <script>
    (function(){
        var url = '/themes/rose/assets/svg/sprite.svg';
        if (!document.getElementById('rose-sprite')) {
            fetch(url).then(function(r){return r.text()}).then(function(txt){
                var div = document.createElement('div');
                div.id = 'rose-sprite';
                div.style.display = 'none';
                div.innerHTML = txt;
                document.addEventListener('DOMContentLoaded', function(){
                    document.body.insertBefore(div, document.body.firstChild);
                });
            }).catch(function(){});
        }
    })();
    </script>
    <style>
        :root {
            /* Rose Garden Color Palette (DB-driven) */
            --rose-deep: {{theme.colors.rose_deep}}{{#unless theme.colors.rose_deep}}#b91c1c{{/unless}};
            --rose-primary: {{theme.colors.rose_primary}}{{#unless theme.colors.rose_primary}}#dc2626{{/unless}};
            --rose-light: {{theme.colors.rose_light}}{{#unless theme.colors.rose_light}}#fecaca{{/unless}};
            --rose-blush: {{theme.colors.rose_blush}}{{#unless theme.colors.rose_blush}}#fef2f2{{/unless}};
            --rose-white: {{theme.colors.rose_white}}{{#unless theme.colors.rose_white}}#fefefe{{/unless}};
            --rose-gold: {{theme.colors.rose_gold}}{{#unless theme.colors.rose_gold}}#d4af37{{/unless}};
            --rose-bronze: {{theme.colors.rose_bronze}}{{#unless theme.colors.rose_bronze}}#cd7f32{{/unless}};
            --rose-text: {{theme.colors.rose_text}}{{#unless theme.colors.rose_text}}#881337{{/unless}};
            --rose-muted: {{theme.colors.rose_muted}}{{#unless theme.colors.rose_muted}}#9f1239{{/unless}};
            --rose-shadow: rgba(185, 28, 28, 0.15);
            --rose-gradient: linear-gradient(135deg, var(--rose-blush) 0%, var(--rose-light) 50%, color-mix(in srgb, var(--rose-light), #fbb6ce 40%) 100%);
            --rose-dark-gradient: linear-gradient(135deg, var(--rose-text) 0%, color-mix(in srgb, var(--rose-text), var(--rose-primary) 40%) 50%, color-mix(in srgb, var(--rose-primary), #ec4899 50%) 100%);

            /* Universal Theme Variables - canonical mapping */
            --theme-bg-primary: {{theme.colors.bg}}{{#unless theme.colors.bg}}var(--rose-blush){{/unless}};
            --theme-bg-secondary: {{theme.colors.bg_light}}{{#unless theme.colors.bg_light}}var(--rose-light){{/unless}};
            --theme-bg-tertiary: {{theme.colors.surface}}{{#unless theme.colors.surface}}var(--rose-white){{/unless}};
            --theme-bg-light: {{theme.colors.bg_light}}{{#unless theme.colors.bg_light}}#f9fafb{{/unless}};
            --theme-bg-surface: {{theme.colors.surface}}{{#unless theme.colors.surface}}var(--rose-light){{/unless}};

            --theme-text-primary: {{theme.colors.text}}{{#unless theme.colors.text}}var(--rose-text){{/unless}};
            --theme-text-secondary: {{theme.colors.text_secondary}}{{#unless theme.colors.text_secondary}}var(--rose-muted){{/unless}};
            --theme-text-muted: {{theme.colors.text_muted}}{{#unless theme.colors.text_muted}}#6b7280{{/unless}};
            --theme-text-subtle: {{theme.colors.text_subtle}}{{#unless theme.colors.text_subtle}}#9ca3af{{/unless}};
            --theme-text-inverted: {{theme.colors.text_inverted}}{{#unless theme.colors.text_inverted}}#fefefe{{/unless}};

            --theme-border-primary: {{theme.colors.border}}{{#unless theme.colors.border}}var(--rose-light){{/unless}};
            --theme-border-light: {{theme.colors.border_light}}{{#unless theme.colors.border_light}}#fee2e2{{/unless}};
            --theme-border-subtle: {{theme.colors.border_subtle}}{{#unless theme.colors.border_subtle}}#f3f4f6{{/unless}};

            --theme-accent: {{theme.colors.accent}}{{#unless theme.colors.accent}}var(--rose-primary){{/unless}};
            --theme-accent-hover: {{theme.colors.accent_hover}}{{#unless theme.colors.accent_hover}}var(--rose-deep){{/unless}};
            --theme-accent-light: {{theme.colors.accent_light}}{{#unless theme.colors.accent_light}}#f87171{{/unless}};
            --theme-success: {{theme.colors.success}}{{#unless theme.colors.success}}#22c55e{{/unless}};
            --theme-warning: {{theme.colors.warning}}{{#unless theme.colors.warning}}#f59e0b{{/unless}};
            --theme-error: {{theme.colors.error}}{{#unless theme.colors.error}}#ef4444{{/unless}};
            
            /* Additional canonical utilities */
            --theme-gradient: linear-gradient(135deg, var(--rose-primary) 0%, var(--rose-light) 50%, color-mix(in srgb, var(--rose-primary), var(--rose-light) 40%) 100%);
            --theme-black-rgb: 0, 0, 0;
            --theme-white-rgb: 255, 255, 255;
            --theme-shadow: rgba(var(--theme-black-rgb), 0.1);
            --theme-shadow-hover: rgba(var(--theme-black-rgb), 0.2);
            --theme-shadow-strong: rgba(var(--theme-black-rgb), 0.8);
            --theme-backdrop: rgba(var(--theme-black-rgb), 0.95);
            --theme-bg-overlay: rgba(var(--theme-black-rgb), 0.05);
            --theme-hero-overlay: rgba(var(--theme-black-rgb), 0.6);
        }
        
        /* Luxury Typography System */
        .luxury-heading {
            font-family: 'Playfair Display', serif;
            font-weight: 700;
            letter-spacing: -0.025em;
        }
        
        /* Standardized heading sizes */
        h1, .luxury-h1 {
            font-size: 3.75rem; /* 60px */
            line-height: 1.1;
            font-weight: 800;
            letter-spacing: -0.025em;
        }
        
        h2, .luxury-h2 {
            font-size: 2.5rem; /* 40px */
            line-height: 1.2;
            font-weight: 700;
            letter-spacing: -0.025em;
        }
        
        h3, .luxury-h3 {
            font-size: 1.875rem; /* 30px */
            line-height: 1.3;
            font-weight: 600;
            letter-spacing: -0.025em;
        }
        
        /* Body text consistency */
        .luxury-text-lg {
            font-size: 1.125rem; /* 18px */
            line-height: 1.6;
        }
        
        .luxury-text-base {
            font-size: 1rem; /* 16px */
            line-height: 1.6;
        }
        
        /* Text contrast helpers for gradient backgrounds */
        .luxury-text-contrast {
            text-shadow: 0 2px 8px rgba(0, 0, 0, 0.3);
        }
        
        .luxury-text-overlay::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.2);
            border-radius: inherit;
            z-index: -1;
        }
        
        /* Responsive typography */
        @media (max-width: 768px) {
            h1, .luxury-h1 { font-size: 2.5rem; }
            h2, .luxury-h2 { font-size: 2rem; }
            h3, .luxury-h3 { font-size: 1.5rem; }
        }
        
        body {
            font-family: 'Poppins', sans-serif;
            background: var(--rose-gradient);
            color: var(--rose-text);
            padding-top: 80px; /* Height of rose navigation (h-20 = 80px) */
            min-height: 100vh;
        }
        
        /* Rose Theme Enhancements */
        .rose-bg-primary { background: var(--rose-primary); }
        .rose-bg-deep { background: var(--rose-deep); }
        .rose-bg-light { background: var(--rose-light); }
        .rose-bg-blush { background: var(--rose-blush); }
        .rose-bg-white { background: var(--rose-white); }
        .rose-bg-gradient { background: var(--rose-gradient); }
        .rose-bg-dark-gradient { background: var(--rose-dark-gradient); }
        
        .rose-text-primary { color: var(--rose-primary); }
        .rose-text-deep { color: var(--rose-deep); }
        .rose-text-gold { color: var(--rose-gold); }
        .rose-text-white { color: var(--rose-white); }

        /* Global container and vertical rhythm */
        .luxury-container { max-width: 1200px; margin-left: auto; margin-right: auto; padding-left: 1rem; padding-right: 1rem; }
        .luxury-section { padding-top: 6rem; padding-bottom: 6rem; }
        @media (max-width: 768px) { .luxury-section { padding-top: 4rem; padding-bottom: 4rem; } }
        .luxury-space-y > * + * { margin-top: 4rem; }
        
        /* Rose Card Styling */
        .luxury-card {
            background: var(--rose-white);
            border-radius: 20px;
            border: 2px solid var(--rose-light);
            box-shadow: 0 20px 40px var(--rose-shadow), 0 1px 3px rgba(185, 28, 28, 0.08);
            transition: all 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
            position: relative;
            overflow: hidden;
        }
        
        .luxury-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: linear-gradient(90deg, var(--rose-primary), var(--rose-gold));
            box-shadow: 0 2px 8px rgba(185, 28, 28, 0.3);
        }
        
        .luxury-card::after {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: url("data:image/svg+xml,%3Csvg width='20' height='20' viewBox='0 0 20 20' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='%23fecaca' fill-opacity='0.03'%3E%3Cpath d='M10 2c4.42 0 8 3.58 8 8s-3.58 8-8 8-8-3.58-8-8 3.58-8 8-8zm0 14c3.31 0 6-2.69 6-6s-2.69-6-6-6-6 2.69-6 6 2.69 6 6 6z'/%3E%3C/g%3E%3C/svg%3E");
            pointer-events: none;
            opacity: 0.5;
        }
        
        .luxury-card:hover {
            transform: translateY(-12px) scale(1.02);
            box-shadow: 0 32px 64px rgba(185, 28, 28, 0.25), 0 1px 3px rgba(185, 28, 28, 0.12);
            border-color: var(--rose-primary);
        }
        
        /* Card variant for dark surfaces */
        .luxury-card-dark {
            background: var(--luxury-card, #131327);
            border-radius: 1rem;
            border: 1px solid rgba(224,184,78,0.15);
            box-shadow: 0 10px 30px rgba(0,0,0,0.35);
        }

        /* Rose Button Styling */
        .luxury-btn {
            background: linear-gradient(135deg, var(--rose-primary) 0%, var(--rose-deep) 100%);
            color: var(--rose-white);
            padding: 16px 32px;
            border-radius: 50px;
            font-weight: 600;
            font-size: 16px;
            text-decoration: none;
            display: inline-flex;
            align-items: center;
            justify-content: center;
            transition: all 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
            box-shadow: 0 8px 32px var(--rose-shadow), 0 2px 8px rgba(185, 28, 28, 0.2);
            border: 2px solid transparent;
            cursor: pointer;
            text-transform: uppercase;
            letter-spacing: 0.5px;
            position: relative;
            overflow: hidden;
        }
        
        .luxury-btn::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);
            transition: left 0.6s;
        }
        
        .luxury-btn:hover::before {
            left: 100%;
        }
        
        .luxury-btn:hover {
            transform: translateY(-4px) scale(1.05);
            box-shadow: 0 16px 48px rgba(185, 28, 28, 0.35), 0 4px 12px rgba(185, 28, 28, 0.25);
            background: linear-gradient(135deg, var(--rose-deep) 0%, var(--rose-primary) 100%);
        }
        
        .luxury-btn-outline {
            background: transparent;
            color: var(--rose-primary);
            border: 2px solid var(--rose-primary);
            padding: 14px 30px;
            border-radius: 50px;
            font-weight: 600;
            font-size: 16px;
            text-decoration: none;
            display: inline-flex;
            align-items: center;
            justify-content: center;
            transition: all 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
            text-transform: uppercase;
            letter-spacing: 0.5px;
            box-shadow: 0 4px 16px rgba(185, 28, 28, 0.1);
        }
        
        .luxury-btn-outline:hover {
            background: var(--rose-primary);
            color: var(--rose-white);
            transform: translateY(-4px) scale(1.05);
            box-shadow: 0 12px 36px rgba(185, 28, 28, 0.3);
            border-color: var(--rose-deep);
        }

        /* Focus visibility for accessibility */
        .luxury-btn:focus-visible, .nav-link:focus-visible, a:focus-visible { outline: 2px solid var(--luxury-accent); outline-offset: 2px; border-radius: 8px; }
        
        /* Rose Navigation */
        .luxury-nav {
            background: linear-gradient(135deg, rgba(185, 28, 28, 0.95) 0%, rgba(136, 19, 55, 0.98) 100%);
            backdrop-filter: blur(20px);
            border-bottom: 2px solid var(--rose-gold);
            box-shadow: 0 4px 24px rgba(185, 28, 28, 0.3), 0 8px 32px rgba(185, 28, 28, 0.15);
            transition: all 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
        }
        
        .luxury-nav .nav-link {
            font-weight: 500;
            color: var(--rose-white);
            text-transform: uppercase;
            letter-spacing: 0.5px;
            font-size: 14px;
            transition: all 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
            position: relative;
            padding: 12px 16px;
            border-radius: 12px;
            display: flex;
            align-items: center;
            justify-content: center;
            min-height: 44px;
            text-shadow: 0 1px 3px rgba(0, 0, 0, 0.3);
        }
        
        .luxury-nav .nav-link::after {
            content: '';
            position: absolute;
            bottom: -2px;
            left: 50%;
            transform: translateX(-50%);
            width: 0;
            height: 3px;
            background: linear-gradient(90deg, var(--rose-gold), var(--rose-bronze));
            transition: width 0.4s cubic-bezier(0.4, 0.0, 0.2, 1);
            border-radius: 2px;
            box-shadow: 0 2px 8px rgba(212, 175, 55, 0.4);
        }
        
        .luxury-nav .nav-link:hover::after,
        .luxury-nav .nav-link.active::after {
            width: 85%;
        }
        
        .luxury-nav .nav-link:hover,
        .luxury-nav .nav-link.active {
            color: var(--rose-gold);
            background: rgba(254, 254, 254, 0.15);
            transform: translateY(-2px);
            box-shadow: 0 4px 12px rgba(185, 28, 28, 0.2);
        }
        
        .luxury-nav .nav-link:focus-visible {
            outline: 2px solid var(--luxury-accent);
            outline-offset: 2px;
            border-radius: 8px;
        }
        
        /* Luxury Brand */
        .luxury-brand {
            font-family: 'Playfair Display', serif;
            font-weight: 800;
            font-size: 28px;
            color: var(--luxury-primary);
            text-decoration: none;
            letter-spacing: -0.5px;
        }
        
        /* Rose Hero Section Styling */
        .luxury-hero {
            background: 
                radial-gradient(ellipse 1200px 800px at 50% -20%, rgba(185, 28, 28, 0.15), transparent 70%),
                linear-gradient(135deg, var(--rose-blush) 0%, var(--rose-light) 50%, #f9a8d4 100%);
            position: relative;
            overflow: hidden;
            border-bottom: 4px solid var(--rose-gold);
        }
        
        .luxury-hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: url("data:image/svg+xml,%3Csvg width='60' height='60' viewBox='0 0 60 60' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='none' fill-rule='evenodd'%3E%3Cg fill='%23dc2626' fill-opacity='0.08'%3E%3Cpath d='M30 20c5.5 0 10 4.5 10 10s-4.5 10-10 10-10-4.5-10-10 4.5-10 10-10zm0 15c2.8 0 5-2.2 5-5s-2.2-5-5-5-5 2.2-5 5 2.2 5 5 5z'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E");
            opacity: 0.6;
        }
        
        .luxury-hero::after {
            content: '';
            position: absolute;
            bottom: -2px;
            left: 0;
            right: 0;
            height: 1px;
            background: linear-gradient(90deg, transparent, var(--rose-gold), transparent);
            box-shadow: 0 0 20px var(--rose-gold);
        }
        
        /* Animations */
        @keyframes luxuryFloat {
            0%, 100% { transform: translateY(0px) rotate(0deg); }
            50% { transform: translateY(-20px) rotate(2deg); }
        }
        
        .luxury-float {
            animation: luxuryFloat 6s ease-in-out infinite;
        }
        
        /* Responsive Design */
        @media (max-width: 768px) {
            .luxury-heading {
                font-size: 2.5rem;
            }
            
            .luxury-card {
                border-radius: 15px;
            }
            
            .luxury-btn {
                padding: 12px 24px;
                font-size: 14px;
            }
        }
        
        /* Custom Scrollbar */
        ::-webkit-scrollbar {
            width: 8px;
        }
        
        ::-webkit-scrollbar-track {
            background: #f1f1f1;
        }
        
        ::-webkit-scrollbar-thumb {
            background: linear-gradient(135deg, var(--luxury-primary), var(--luxury-secondary));
            border-radius: 4px;
        }
        
        ::-webkit-scrollbar-thumb:hover {
            background: linear-gradient(135deg, var(--luxury-secondary), var(--luxury-primary));
        }

.luxury-gallery-section { background: var(--luxury-surface); }
.luxury-gallery-grid { display: grid; grid-template-columns: repeat(5, 1fr); gap: 1rem; }
.luxury-gallery-item { overflow: hidden; border-radius: 0.75rem; }
.luxury-gallery-item img { transition: transform 0.3s ease; }
.luxury-gallery-item:hover img { transform: scale(1.05); }
.luxury-gallery-placeholder { background: var(--luxury-card); color: var(--luxury-muted); }

.luxury-travel-section { background: var(--luxury-surface); }
.luxury-travel-grid { display: grid; grid-template-columns: repeat(3, 1fr); gap: 2rem; }
.luxury-travel-card { text-align: center; }
.luxury-available-badge { background: var(--luxury-accent); color: #fff; padding: 0.5rem 1rem; border-radius: 9999px; }
.luxury-travel-notes { font-style: italic; color: var(--luxury-muted); }

.luxury-testimonials-section { background: var(--luxury-background); }
.luxury-testimonials-grid { display: grid; grid-template-columns: repeat(3, 1fr); gap: 2rem; }
.luxury-stars { display: flex; justify-content: center; gap: 0.25rem; }
.luxury-testimonial-text { font-style: italic; }
.luxury-testimonial-author { color: var(--luxury-accent); }

/* Responsive Media Queries */
@media (max-width: 1024px) {
  .luxury-gallery-grid { grid-template-columns: repeat(3, 1fr); }
  .luxury-travel-grid, .luxury-testimonials-grid { grid-template-columns: repeat(2, 1fr); }
}
@media (max-width: 768px) {
  .luxury-gallery-grid { grid-template-columns: repeat(2, 1fr); }
  .luxury-travel-grid, .luxury-testimonials-grid { grid-template-columns: 1fr; }
}

        /* Universal Theme Utility Classes */
        /* Background Classes */
        .theme-bg-primary { background-color: var(--theme-bg-primary) !important; }
        .theme-bg-secondary { background-color: var(--theme-bg-secondary) !important; }
        .theme-bg-tertiary { background-color: var(--theme-bg-tertiary) !important; }
        .theme-bg-light { background-color: var(--theme-bg-light) !important; }
        
        /* Text Color Classes */
        .theme-text-primary { color: var(--theme-text-primary) !important; }
        .theme-text-secondary { color: var(--theme-text-secondary) !important; }
        .theme-text-muted { color: var(--theme-text-muted) !important; }
        .theme-text-subtle { color: var(--theme-text-subtle) !important; }
        
        /* Border Classes */
        .theme-border-primary { border-color: var(--theme-border-primary) !important; }
        .theme-border-light { border-color: var(--theme-border-light) !important; }
        
        /* Accent Classes */
        .theme-accent { color: var(--theme-accent) !important; }
        .theme-accent-bg { background-color: var(--theme-accent) !important; }
        .theme-accent-border { border-color: var(--theme-accent) !important; }
        
        /* Semantic Component Classes */
        .theme-card {
            background-color: var(--theme-bg-tertiary);
            color: var(--theme-text-primary);
            border: 1px solid var(--theme-border-light);
            border-radius: 12px;
            box-shadow: 0 10px 30px rgba(0,0,0,0.1);
            transition: all 0.3s ease;
        }
        
        .theme-card:hover {
            box-shadow: 0 20px 40px rgba(0,0,0,0.15);
            transform: translateY(-2px);
        }
        
        .theme-section {
            background-color: var(--theme-bg-primary);
            color: var(--theme-text-primary);
        }
        
        .theme-section-alt {
            background-color: var(--theme-bg-secondary);
            color: var(--theme-text-primary);
        }
        
        .theme-header {
            color: var(--theme-text-primary);
            font-weight: bold;
        }
        
        .theme-body-text {
            color: var(--theme-text-secondary);
        }
        
        .theme-muted-text {
            color: var(--theme-text-muted);
        }
        
        .theme-subtle-text {
            color: var(--theme-text-subtle);
        }
    </style>
    
    <!-- Rose Theme Enhancements -->
    <link rel="stylesheet" href="/themes/rose/assets/rose-enhancements.css">
    <link rel="stylesheet" href="/themes/rose/assets/rose-responsive.css">

    <!-- Dynamic theme color CSS - Load AFTER inline styles so variables override -->
    <link rel="stylesheet" href="/api/theme-colors/{{#if previewThemeId}}{{previewThemeId}}{{else}}{{themeId}}{{/if}}/css?v={{timestamp}}&force=1{{#if preview.previewPaletteId}}&preview_palette={{preview.previewPaletteId}}{{/if}}">
</head>
<body>
    <a href="#main-content" class="sr-only focus:not-sr-only focus:fixed focus:top-2 focus:left-2 focus:bg-white focus:text-black focus:px-3 focus:py-2 focus:rounded focus:z-50">Skip to main content</a>
    <!-- Navigation -->
    {{> navigation}}
    
    <!-- Theme Preview Banner -->
    {{#if theme.isPreview}}
    <div class="bg-gradient-to-r from-amber-500 to-yellow-600 text-white py-2 px-4 text-center text-sm font-semibold">
        <i class="fas fa-eye me-2"></i>You are previewing the <strong>{{theme.previewThemeName}}</strong> theme. This is not your active theme.
    </div>
    {{/if}}
    
    <!-- Main Content -->
    <main id="main-content">
        {{{body}}}
    </main>
    
    <!-- Footer -->
    {{> footer}}
    
    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    
    <script>
        // Initialize AOS
        AOS.init({
            duration: 800,
            easing: 'ease-in-out',
            once: true,
            offset: 100
        });
        
        // Luxury floating elements
        document.addEventListener('DOMContentLoaded', function() {
            const floatingElements = document.querySelectorAll('.luxury-float');
            floatingElements.forEach((el, index) => {
                el.style.animationDelay = `${index * 0.5}s`;
            });
        });
        
        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });
        
        // Luxury navigation background on scroll
        window.addEventListener('scroll', function() {
            const nav = document.querySelector('.luxury-nav');
            if (nav) {
                if (window.scrollY > 50) {
                    nav.style.background = 'rgba(255,255,255,0.98)';
                    nav.style.boxShadow = '0 8px 32px rgba(0,0,0,0.15)';
                } else {
                    nav.style.background = 'rgba(255,255,255,0.95)';
                    nav.style.boxShadow = '0 4px 24px rgba(0,0,0,0.1)';
                }
            }
        });
    </script>
</body>
</html>