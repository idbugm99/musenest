<!-- Gallery Preview Section -->
{{#if content.gallery_section_visible}}
{{#if galleryImages}}
<section class="py-20 animate-on-scroll" style="background-color: var(--theme-bg);">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-16">
            <h2 class="text-4xl md:text-5xl font-bold mb-6 luxury-gold-text" 
                style="font-family: var(--font-display);">
                {{#if content.gallery_section_title}}{{content.gallery_section_title}}{{else}}Portfolio{{/if}}
            </h2>
            <p class="text-xl max-w-3xl mx-auto leading-relaxed" style="color: var(--theme-text-subtle);">
                A curated collection of professional photography showcasing elegance and sophistication.
            </p>
        </div>
        
        <!-- Gallery Preview Grid -->
        {{#if galleryImages}}
        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4 mb-12">
            {{#each galleryImages}}
            <div class="luxury-card overflow-hidden aspect-square group animate-luxury-fade-in" style="animation-delay: {{multiply @index 0.1}}s;">
                <img src="/uploads/{{../modelSlug}}/public/gallery/{{this.filename}}" 
                     alt="{{#if this.caption}}{{this.caption}}{{else}}Gallery image{{/if}}"
                     class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-110">
                <div class="absolute inset-0 luxury-gradient opacity-0 group-hover:opacity-40 transition-opacity duration-300 flex items-center justify-center">
                    <i class="fas fa-search-plus text-white text-2xl"></i>
                </div>
            </div>
            {{/each}}
        </div>
        {{else}}
        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4 mb-12">
            {{#times 5}}
            <div class="luxury-card overflow-hidden aspect-square group animate-luxury-fade-in" style="animation-delay: {{multiply this 0.1}}s;">
                <div class="w-full h-full bg-gradient-to-br from-gray-200 to-gray-300 flex items-center justify-center">
                    <i class="fas fa-image text-gray-400 text-3xl"></i>
                </div>
            </div>
            {{/times}}
        </div>
        {{/if}}
        
        <div class="text-center">
            <a href="/{{modelSlug}}/gallery{{previewParam}}" 
               class="luxury-btn text-lg px-8 py-4">
                <i class="fas fa-images"></i>
                View Full Portfolio
            </a>
        </div>
    </div>
</section>
{{/if}}
{{/if}}

<!-- Travel/Calendar Section -->
{{#if content.travel_section_visible}}
<section class="py-20 animate-on-scroll" style="background-color: var(--theme-bg-alt);">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-16">
            <h2 class="text-4xl md:text-5xl font-bold mb-6 luxury-gold-text" 
                style="font-family: var(--font-display);">
                {{#if content.travel_section_title}}{{content.travel_section_title}}{{else}}Location Availability{{/if}}
            </h2>
            <p class="text-xl max-w-3xl mx-auto leading-relaxed" style="color: var(--theme-text-subtle);">
                Schedule your exclusive experience in these premier destinations.
            </p>
        </div>
        
        {{#if upcomingEvents}}
        <div class="grid grid-cols-1 md:grid-cols-{{#if (gt upcomingEvents.length 2)}}3{{else}}2{{/if}} gap-8 justify-center">
            {{#each upcomingEvents}}
            {{#if (lt @index 3)}}
            <div class="luxury-card p-8 text-center animate-luxury-scale" style="animation-delay: {{multiply @index 0.1}}s;">
                <div class="w-16 h-16 luxury-gradient rounded-full flex items-center justify-center mx-auto mb-6">
                    <i class="fas fa-map-marker-alt text-white text-xl"></i>
                </div>
                <h3 class="text-2xl font-semibold mb-2" style="font-family: var(--font-display); color: var(--theme-text);">
                    {{this.location}}
                </h3>
                <p class="mb-4 text-lg" style="color: var(--theme-text-subtle);">
                    {{this.dateRange}}
                </p>
                {{#if this.notes}}
                <p class="text-sm italic" style="color: var(--theme-text-subtle);">
                    {{this.notes}}
                </p>
                {{/if}}
                <div class="mt-4">
                    <span class="inline-block bg-luxury-primary/10 text-luxury-primary px-3 py-1 rounded-full text-sm font-medium">
                        Available Now
                    </span>
                </div>
            </div>
            {{/if}}
            {{/each}}
        </div>
        {{else}}
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
            {{#times 3}}
            <div class="luxury-card p-8 text-center animate-luxury-scale" style="animation-delay: {{multiply this 0.1}}s;">
                <div class="w-16 h-16 luxury-gradient rounded-full flex items-center justify-center mx-auto mb-6">
                    <i class="fas fa-map-marker-alt text-white text-xl"></i>
                </div>
                <h3 class="text-xl font-semibold mb-2" style="font-family: var(--font-display); color: var(--theme-text);">
                    {{#if (eq this 1)}}New York{{/if}}
                    {{#if (eq this 2)}}London{{/if}}
                    {{#if (eq this 3)}}Paris{{/if}}
                </h3>
                <p class="mb-4" style="color: var(--theme-text-subtle);">
                    Coming Soon
                </p>
            </div>
            {{/times}}
        </div>
        {{/if}}
        
        <div class="text-center mt-12">
            <a href="/{{modelSlug}}/{{#if content.travel_cta_link}}{{content.travel_cta_link}}{{else}}calendar{{/if}}{{previewParam}}" 
               class="luxury-btn">
                <i class="fas fa-calendar-alt"></i>
                {{#if content.travel_cta_text}}{{content.travel_cta_text}}{{else}}View Full Calendar{{/if}}
            </a>
        </div>
    </div>
</section>
{{/if}}

