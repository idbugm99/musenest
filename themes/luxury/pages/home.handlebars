<!-- Luxury Home Page -->

<!-- Hero Section -->
<section class="luxury-hero-section" style="{{#if content.heroBackgroundImageUrl}}background: #2C1810;{{else}}background: linear-gradient(135deg, #D4AF37, #8B4513);{{/if}} min-height: 100vh; position: relative; overflow: hidden;">
    
    <!-- Luxury Decorative Elements -->
    <div style="position: absolute; top: 15%; left: 8%; opacity: 0.3; z-index: 1;">
        <div style="width: 4rem; height: 4rem; border: 2px solid rgba(212, 175, 55, 0.6); border-radius: 50%; animation: luxuryFloat 8s ease-in-out infinite;"></div>
    </div>
    <div style="position: absolute; top: 25%; right: 12%; opacity: 0.4; z-index: 1;">
        <div style="width: 3rem; height: 3rem; background: rgba(212, 175, 55, 0.3); border-radius: 50%; animation: luxuryFloat 10s ease-in-out infinite; animation-delay: 3s;"></div>
    </div>
    <div style="position: absolute; bottom: 30%; left: 75%; opacity: 0.5; z-index: 1;">
        <div style="width: 2rem; height: 2rem; border: 2px solid rgba(212, 175, 55, 0.4); border-radius: 50%; animation: luxuryFloat 12s ease-in-out infinite; animation-delay: 6s;"></div>
    </div>

    <!-- Parallax Background Image -->
    {{#if content.heroBackgroundImageUrl}}
    <div style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; background-image: url('{{content.heroBackgroundImageUrl}}'); background-size: cover; background-position: center; background-attachment: fixed; opacity: {{#if content.hero_background_opacity}}{{content.hero_background_opacity}}{{else}}0.7{{/if}}; z-index: 0;"></div>
    {{/if}}
    
    <div class="luxury-container" style="position: relative; z-index: 2; padding-top: 8rem; padding-bottom: 8rem; max-width: 1200px; margin: 0 auto; padding-left: 2rem; padding-right: 2rem;">
        <!-- Hero Section -->
        <div style="text-align: center; margin-bottom: 6rem;">
            <div style="margin-bottom: 3rem;">
                <div style="width: 5rem; height: 5rem; background: linear-gradient(45deg, #D4AF37, #B8941F); border-radius: 50%; margin: 0 auto; display: flex; align-items: center; justify-content: center; color: white; font-size: 2rem; animation: luxuryPulse 4s ease-in-out infinite;">
                    <i class="fas fa-crown"></i>
                </div>
            </div>
            
            <h1 style="font-size: clamp(3rem, 8vw, 6rem); color: #F5F5DC; margin-bottom: 2rem; font-family: Georgia, serif; font-weight: 700; text-shadow: 0 4px 20px rgba(0,0,0,0.5); line-height: 1.1;">
                {{#if content.hero_title}}{{content.hero_title}}{{else}}{{#if model.name}}{{model.name}}{{else}}{{modelName}}{{/if}}{{/if}}
            </h1>
            
            {{#if content.hero_subtitle}}
            <p style="font-size: 1.5rem; color: rgba(245, 245, 220, 0.9); max-width: 800px; margin: 0 auto 1.5rem; line-height: 1.6; text-shadow: 0 2px 10px rgba(0,0,0,0.3);">
                {{content.hero_subtitle}}
            </p>
            {{else}}
            <p style="font-size: 1.5rem; color: rgba(245, 245, 220, 0.9); max-width: 800px; margin: 0 auto 1.5rem; line-height: 1.6; text-shadow: 0 2px 10px rgba(0,0,0,0.3);">
                {{#if tagline}}{{tagline}}{{else}}Experience elegance, sophistication, and unforgettable moments with a refined companion.{{/if}}
            </p>
            {{/if}}
            
            {{#if content.hero_description}}
            <div style="font-size: 1.1rem; color: rgba(245, 245, 220, 0.8); max-width: 700px; margin: 0 auto 3rem; line-height: 1.6; text-shadow: 0 1px 5px rgba(0,0,0,0.3);">
                {{{content.hero_description}}}
            </div>
            {{else}}
            <div style="margin-bottom: 3rem;"></div>
            {{/if}}
            
            <div style="display: flex; gap: 2rem; justify-content: center; flex-wrap: wrap;">
                {{#if content.hero_button_1_text}}
                <a href="{{#if content.hero_button_1_link}}/{{modelSlug}}/{{content.hero_button_1_link}}{{previewParam}}{{else}}/{{modelSlug}}/contact{{previewParam}}{{/if}}" class="luxury-hero-btn" style="font-size: 1.2rem; padding: 1.2rem 2.5rem;">
                    <i class="fas fa-calendar-alt" style="margin-right: 0.8rem;"></i>
                    {{content.hero_button_1_text}}
                </a>
                {{else}}
                <a href="/{{modelSlug}}/contact{{previewParam}}" class="luxury-hero-btn" style="font-size: 1.2rem; padding: 1.2rem 2.5rem;">
                    <i class="fas fa-envelope" style="margin-right: 0.8rem;"></i>
                    Book Now
                </a>
                {{/if}}
                
                {{#if content.hero_button_2_text}}
                <a href="{{#if content.hero_button_2_link}}/{{modelSlug}}/{{content.hero_button_2_link}}{{previewParam}}{{else}}/{{modelSlug}}/about{{previewParam}}{{/if}}" class="luxury-hero-btn-outline" style="font-size: 1.2rem; padding: 1.2rem 2.5rem;">
                    <i class="fas fa-user" style="margin-right: 0.8rem;"></i>
                    {{content.hero_button_2_text}}
                </a>
                {{else}}
                <a href="/{{modelSlug}}/about{{previewParam}}" class="luxury-hero-btn-outline" style="font-size: 1.2rem; padding: 1.2rem 2.5rem;">
                    <i class="fas fa-user" style="margin-right: 0.8rem;"></i>
                    View Services
                </a>
                {{/if}}
            </div>
        </div>
    </div>
</section>

<!-- About Preview Section -->
{{#if content.about_section_visible}}
<section class="py-20 animate-on-scroll" style="background-color: var(--theme-bg);">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        {{#if content.portrait_section_visible}}
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-16 items-center">
            <!-- About Image -->
            {{#if content.portrait_image_url}}
            <div class="relative animate-luxury-fade-in">
                <div class="luxury-card overflow-hidden aspect-[4/5]">
                    <img src="{{content.portrait_image_url}}" 
                         alt="{{#if content.portrait_alt}}{{content.portrait_alt}}{{else}}Professional Portrait{{/if}}"
                         class="w-full h-full object-cover">
                </div>
                <div class="absolute -bottom-6 -right-6 w-24 h-24 luxury-gradient rounded-full flex items-center justify-center">
                    <i class="fas fa-crown text-white text-2xl"></i>
                </div>
            </div>
            {{else}}
            <!-- Fallback if no portrait image -->
            <div class="relative animate-luxury-fade-in">
                <div class="luxury-card overflow-hidden aspect-[4/5] bg-gradient-to-br from-luxury-light to-luxury-secondary flex items-center justify-center">
                    <i class="fas fa-user text-luxury-primary text-6xl opacity-50"></i>
                </div>
                <div class="absolute -bottom-6 -right-6 w-24 h-24 luxury-gradient rounded-full flex items-center justify-center">
                    <i class="fas fa-crown text-white text-2xl"></i>
                </div>
            </div>
            {{/if}}
            
            <!-- About Content -->
            <div class="animate-luxury-slide-in" style="animation-delay: 0.2s;">
                <h2 class="text-4xl md:text-5xl font-bold mb-6 luxury-gold-text" 
                    style="font-family: var(--font-display);">
                    {{#if content.about_title}}{{content.about_title}}{{else}}About {{siteName}}{{/if}}
                </h2>
                
                {{#if content.about_paragraph_1}}
                <p class="text-xl leading-relaxed mb-6" style="color: var(--theme-text-subtle);">
                    {{content.about_paragraph_1}}
                </p>
                {{/if}}
                
                {{#if content.about_paragraph_2}}
                <p class="text-lg leading-relaxed mb-8" style="color: var(--theme-text-subtle);">
                    {{content.about_paragraph_2}}
                </p>
                {{/if}}
                
                {{#unless content.about_paragraph_1}}
                <p class="text-xl leading-relaxed mb-6" style="color: var(--theme-text-subtle);">
                    Welcome to my world of elegance and sophistication. I am a refined companion who values discretion, intelligence, and genuine connection.
                </p>
                <p class="text-lg leading-relaxed mb-8" style="color: var(--theme-text-subtle);">
                    With years of experience in providing exclusive companionship services, I bring understanding, intimacy, and unforgettable moments to each encounter.
                </p>
                {{/unless}}
                
                <div class="luxury-divider"></div>
                
                {{#if content.about_link_text}}
                    {{#if (eq content.about_link_destination 'about')}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="luxury-btn-outline">
                            <i class="fas fa-user-circle"></i>
                            {{content.about_link_text}}
                        </a>
                    {{else}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="luxury-btn-outline">
                            <i class="fas fa-envelope"></i>
                            {{content.about_link_text}}
                        </a>
                    {{/if}}
                {{else}}
                    <a href="/{{modelSlug}}/about{{previewParam}}" class="luxury-btn-outline">
                        <i class="fas fa-user-circle"></i>
                        Learn More About Me
                    </a>
                {{/if}}
            </div>
        </div>
        {{else}}
        <div class="text-center max-w-4xl mx-auto">
            <h2 class="text-4xl md:text-5xl font-bold mb-6 luxury-gold-text" 
                style="font-family: var(--font-display);">
                {{#if content.about_title}}{{content.about_title}}{{else}}About {{siteName}}{{/if}}
            </h2>
            {{#if content.about_paragraph_1}}
            <p class="text-xl leading-relaxed mb-6" style="color: var(--theme-text-subtle);">
                {{content.about_paragraph_1}}
            </p>
            {{/if}}
            {{#if content.about_paragraph_2}}
            <p class="text-lg leading-relaxed mb-8" style="color: var(--theme-text-subtle);">
                {{content.about_paragraph_2}}
            </p>
            {{/if}}
            {{#if content.about_link_text}}
                <a href="/{{modelSlug}}/about{{previewParam}}" style="color: var(--theme-accent); font-weight: 600;">{{content.about_link_text}} →</a>
            {{else}}
                <a href="/{{modelSlug}}/about{{previewParam}}" style="color: var(--theme-accent); font-weight: 600;">Learn More About Me →</a>
            {{/if}}
        </div>
        {{/if}}
    </div>
</section>
{{/if}}

