<!-- Luxury Theme Rates Page - Royal Pricing Palace -->

<!-- Royal Rates Header with Imperial Crown -->
{{#if rates_content.rates_header_visible}}
<section class="luxury-rates-header" data-aos="fade-in">
    <!-- Imperial Marble Background -->
    <div class="luxury-rates-marble-bg"></div>
    
    <!-- Floating Gold Coins -->
    <div class="luxury-rates-coins">
        <div class="luxury-coin luxury-coin-1">$</div>
        <div class="luxury-coin luxury-coin-2">$</div>
        <div class="luxury-coin luxury-coin-3">$</div>
    </div>
    
    <div class="max-w-6xl mx-auto text-center px-4 sm:px-6 lg:px-8">
        <!-- Imperial Crown -->
        <div class="luxury-rates-crown" data-aos="fade-down" data-aos-delay="200">
            <i class="fas fa-crown"></i>
        </div>
        
        <!-- Royal Title -->
        <h1 class="luxury-rates-title" data-aos="fade-up" data-aos-delay="400">
            {{#if rates_content.page_title}}{{rates_content.page_title}}{{else}}Royal Investment{{/if}}
        </h1>
        
        <!-- Divider Section -->
        {{#if rates_content.hero_divider_visible}}
        <div class="luxury-rates-divider" data-aos="fade-up" data-aos-delay="600">
            {{#if (eq rates_content.hero_divider_type 'line')}}
            <div class="luxury-divider-line"></div>
            {{else}}
            <div class="luxury-divider-icon">
                <i class="{{#if rates_content.hero_divider_icon}}{{rates_content.hero_divider_icon}}{{else}}fas fa-heart{{/if}}"></i>
            </div>
            {{/if}}
        </div>
        {{/if}}
        
        <!-- Royal Subtitle with Enhanced Customization -->
        <p class="luxury-rates-subtitle" data-aos="fade-up" data-aos-delay="800"
           style="{{#if rates_content.hero_subtitle_color}}color: {{rates_content.hero_subtitle_color}};{{/if}}
                  {{#if (eq rates_content.hero_subtitle_size 'small')}}font-size: 1.2rem;{{else}}{{#if (eq rates_content.hero_subtitle_size 'large')}}font-size: 2rem;{{else}}font-size: 1.6rem;{{/if}}{{/if}}">
            {{#if rates_content.page_subtitle}}{{rates_content.page_subtitle}}{{else}}Investment in time together. All arrangements are for companionship and social time only.{{/if}}
        </p>
    </div>
    
    <!-- Palace Ornaments -->
    <div class="luxury-rates-ornaments" aria-hidden="true">
        <div class="luxury-ornament luxury-ornament-1">⚜</div>
        <div class="luxury-ornament luxury-ornament-2">♦</div>
        <div class="luxury-ornament luxury-ornament-3">⚜</div>
    </div>
</section>
{{/if}}

<!-- Page Section Navigation (Sticky below main nav) -->
<div class="luxury-section-navigation sticky top-20 bg-white/95 backdrop-blur-md border-b border-luxury-light shadow-sm z-40 mt-4">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="flex flex-wrap justify-center gap-4 py-3">
            {{#if rates_content.rates_table_visible}}
            <a href="#rates-palace" class="luxury-section-nav-link">
                <i class="fas fa-coins text-sm"></i>
                <span>{{#if rates_content.nav_rates_label}}{{rates_content.nav_rates_label}}{{else}}Rates{{/if}}</span>
            </a>
            {{/if}}
            {{#if rates.extended.length}}
            <a href="#extended-palace" class="luxury-section-nav-link">
                <i class="fas fa-gem text-sm"></i>
                <span>{{#if rates_content.nav_extended_label}}{{rates_content.nav_extended_label}}{{else}}Extended{{/if}}</span>
            </a>
            {{/if}}
            {{#if rates_content.rates_additional_visible}}
            <a href="#services-palace" class="luxury-section-nav-link">
                <i class="fas fa-crown text-sm"></i>
                <span>{{#if rates_content.nav_services_label}}{{rates_content.nav_services_label}}{{else}}Services{{/if}}</span>
            </a>
            {{/if}}
            <a href="#policies-palace" class="luxury-section-nav-link">
                <i class="fas fa-scroll text-sm"></i>
                <span>{{#if rates_content.nav_policies_label}}{{rates_content.nav_policies_label}}{{else}}Policies{{/if}}</span>
            </a>
            {{#if rates_content.rates_payment_visible}}
            <a href="#payment-palace" class="luxury-section-nav-link">
                <i class="fas fa-credit-card text-sm"></i>
                <span>{{#if rates_content.nav_payment_label}}{{rates_content.nav_payment_label}}{{else}}Payment{{/if}}</span>
            </a>
            {{/if}}
            {{#if rates_content.cta_visible}}
            <a href="#contact-section" class="luxury-section-nav-link">
                <i class="fas fa-envelope text-sm"></i>
                <span>{{#if rates_content.nav_contact_label}}{{rates_content.nav_contact_label}}{{else}}Contact{{/if}}</span>
            </a>
            {{/if}}
        </div>
    </div>
</div>

<!-- Royal Rates Palace Section -->
{{#if rates_content.rates_table_visible}}
<section id="rates-palace" class="luxury-rates-palace" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        
        <!-- Royal Rates Table Header -->
        {{#if rates_content.table_title}}
        <div class="luxury-table-header text-center mb-12" data-aos="fade-up">
            <h2 class="luxury-table-title">{{rates_content.table_title}}</h2>
            {{#if rates_content.table_description}}
            <p class="luxury-table-description">{{rates_content.table_description}}</p>
            {{/if}}
        </div>
        {{/if}}
        <div class="grid lg:grid-cols-2 gap-12">
            
            <!-- Incall Services -->
            {{#if rates_content.rates_incall_visible}}
            <div class="luxury-rate-chamber" data-aos="fade-up" data-aos-delay="100"
                 {{#if rates_content.rates_background_tint_visible}}style="background-color: {{#if rates_content.rates_background_tint_color}}{{rates_content.rates_background_tint_color}}{{else}}var(--bg-light){{/if}};"{{/if}}>
                <!-- Palace Dome -->
                <div class="luxury-chamber-dome"></div>
                
                <!-- Chamber Header -->
                <div class="luxury-chamber-header luxury-incall-header">
                    <div class="luxury-header-crown">
                        <i class="fas fa-crown"></i>
                    </div>
                    <h2 class="luxury-chamber-title">Incall Services</h2>
                </div>
                
                <!-- Rate Items -->
                <div class="luxury-chamber-content">
                    {{#each rates.incall}}
                    <div class="luxury-rate-item">
                        <div class="luxury-rate-details">
                            <div class="luxury-rate-name">
                                {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                
                                <!-- Highlight Badge with Database Control -->
                                {{#if ../rates_content.rates_highlight_badge_visible}}
                                {{#if this.highlightBadge}}
                                <span class="luxury-highlight-badge">
                                    <i class="fas fa-star"></i>
                                    {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}{{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}{{/if}}
                                </span>
                                {{else}}{{#if this.isMostPopular}}
                                <span class="luxury-highlight-badge">
                                    <i class="fas fa-star"></i>
                                    {{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}
                                </span>
                                {{/if}}{{/if}}
                                {{/if}}
                            </div>
                            {{#if this.serviceName}}
                            <div class="luxury-rate-duration">{{this.duration}}</div>
                            {{/if}}
                        </div>
                        
                        <div class="luxury-royal-dots"></div>
                        <div class="luxury-rate-price luxury-incall-price">${{this.price}}</div>
                    </div>
                    {{/each}}
                </div>
                
                <!-- Marble Base -->
                <div class="luxury-chamber-base"></div>
            </div>
            {{/if}}
            
            <!-- Outcall Services -->
            {{#if rates_content.rates_outcall_visible}}
            <div class="luxury-rate-chamber" data-aos="fade-up" data-aos-delay="200"
                 {{#if rates_content.rates_background_tint_visible}}style="background-color: {{#if rates_content.rates_background_tint_color}}{{rates_content.rates_background_tint_color}}{{else}}var(--bg-light){{/if}};"{{/if}}>
                <!-- Palace Dome -->
                <div class="luxury-chamber-dome"></div>
                
                <!-- Chamber Header -->
                <div class="luxury-chamber-header luxury-outcall-header">
                    <div class="luxury-header-crown">
                        <i class="fas fa-crown"></i>
                    </div>
                    <h2 class="luxury-chamber-title">Outcall Services</h2>
                </div>
                
                <!-- Rate Items -->
                <div class="luxury-chamber-content">
                    {{#each rates.outcall}}
                    <div class="luxury-rate-item">
                        <div class="luxury-rate-details">
                            <div class="luxury-rate-name">
                                {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                
                                <!-- Highlight Badge with Database Control -->
                                {{#if ../rates_content.rates_highlight_badge_visible}}
                                {{#if this.highlightBadge}}
                                <span class="luxury-highlight-badge">
                                    <i class="fas fa-star"></i>
                                    {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}{{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}{{/if}}
                                </span>
                                {{else}}{{#if this.isMostPopular}}
                                <span class="luxury-highlight-badge">
                                    <i class="fas fa-star"></i>
                                    {{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}
                                </span>
                                {{/if}}{{/if}}
                                {{/if}}
                            </div>
                            {{#if this.serviceName}}
                            <div class="luxury-rate-duration">{{this.duration}}</div>
                            {{/if}}
                        </div>
                        
                        <div class="luxury-royal-dots"></div>
                        <div class="luxury-rate-price luxury-outcall-price">${{this.price}}</div>
                    </div>
                    {{/each}}
                </div>
                
                <!-- Marble Base -->
                <div class="luxury-chamber-base"></div>
            </div>
            {{/if}}
        </div>
    </div>
</section>
{{/if}}

<!-- Extended Palace Engagements -->
{{#if rates_content.rates_extended_visible}}
{{#if rates.extended.length}}
<section id="extended-palace" class="luxury-extended-palace" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <!-- Royal Section Header with Database Control -->
        <div class="luxury-section-header text-center mb-16">
            <div class="luxury-section-crown">
                <i class="fas fa-crown"></i>
            </div>
            <h2 class="luxury-section-title">
                {{#if rates_content.extended_title}}{{rates_content.extended_title}}{{else}}Extended Royal Engagements{{/if}}
            </h2>
            {{#if rates_content.extended_description}}
            <p class="luxury-section-description">{{rates_content.extended_description}}</p>
            {{/if}}
            <div class="luxury-ornate-separator">
                <div class="luxury-ornate-line"></div>
                <div class="luxury-ornate-center">♦</div>
                <div class="luxury-ornate-line"></div>
            </div>
        </div>
        
        <!-- Extended Rate Palaces -->
        <div class="grid md:grid-cols-3 gap-10">
            {{#each rates.extended}}
            <div class="luxury-extended-chamber" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <!-- Extended Palace Dome -->
                <div class="luxury-extended-dome"></div>
                
                <!-- Extended Header -->
                <div class="luxury-extended-header">
                    {{#if ../rates_content.extended_icon_visible}}
                    <div class="luxury-extended-icon">
                        <i class="{{#if this.rateIcon}}{{this.rateIcon}}{{else}}{{#eq this.serviceName 'Weekend'}}fas fa-calendar-weekend{{else}}{{#eq this.serviceName 'Week'}}fas fa-calendar-week{{else}}fas fa-plane{{/eq}}{{/eq}}{{/if}}"></i>
                    </div>
                    {{/if}}
                    {{#if ../rates_content.extended_show_service_name}}
                    <h3 class="luxury-extended-title">{{this.serviceName}}</h3>
                    {{/if}}
                </div>
                
                <!-- Extended Content -->
                <div class="luxury-extended-content">
                    {{#if ../rates_content.extended_show_price}}
                    <div class="luxury-extended-price">${{this.price}}</div>
                    {{/if}}
                    {{#if ../rates_content.extended_show_duration}}
                    <div class="luxury-extended-duration">{{this.duration}}</div>
                    {{/if}}
                    
                    {{#if ../rates_content.extended_description_visible}}
                    {{#if this.rateDescription}}
                    <p class="luxury-extended-description">{{this.rateDescription}}</p>
                    {{/if}}
                    {{/if}}
                </div>
                
                <!-- Extended Marble Base -->
                <div class="luxury-extended-base"></div>
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/if}}
{{/if}}

<!-- Royal Services & Policies Palace -->
<section id="services-palace" class="luxury-services-policies-palace" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid lg:grid-cols-2 gap-12">
            
            <!-- Additional Royal Services -->
            {{#if rates_content.rates_additional_visible}}
            <div class="luxury-service-palace" data-aos="fade-up" data-aos-delay="100">
                <!-- Service Palace Header -->
                <div class="luxury-service-header">
                    <div class="luxury-service-crown">
                        <i class="fas fa-plus-circle"></i>
                    </div>
                    <h3 class="luxury-service-title">Additional Royal Services</h3>
                </div>
                
                <!-- Service List with Database Integration -->
                <div class="luxury-service-list">
                    {{#if additionalServices.length}}
                    {{#each additionalServices}}
                    <div class="luxury-service-item">
                        <div class="luxury-service-ornament">♦</div>
                        <span class="luxury-service-text">{{this.service_name}}</span>
                        {{#if this.price}}<span class="luxury-service-price">{{this.price}}</span>{{/if}}
                        {{#if this.description}}<div class="luxury-service-description">{{this.description}}</div>{{/if}}
                    </div>
                    {{/each}}
                    {{else if rates_content.additional_services}}
                    {{#each (split rates_content.additional_services " • ")}}
                    <div class="luxury-service-item">
                        <div class="luxury-service-ornament">♦</div>
                        <span class="luxury-service-text">{{this}}</span>
                    </div>
                    {{/each}}
                    {{else}}
                    <div class="luxury-service-item">
                        <div class="luxury-service-ornament">♦</div>
                        <span class="luxury-service-text">Dinner dates: +$100 (restaurant cost separate)</span>
                    </div>
                    <div class="luxury-service-item">
                        <div class="luxury-service-ornament">♦</div>
                        <span class="luxury-service-text">Travel outside city: +$500/day + expenses</span>
                    </div>
                    <div class="luxury-service-item">
                        <div class="luxury-service-ornament">♦</div>
                        <span class="luxury-service-text">Special occasions and events welcomed</span>
                    </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
            
            <!-- Royal Booking Policies -->
            <div id="policies-palace" class="luxury-policy-palace" data-aos="fade-up" data-aos-delay="200">
                <!-- Policy Palace Header -->
                <div class="luxury-policy-header">
                    <div class="luxury-policy-crown">
                        <i class="fas fa-calendar-check"></i>
                    </div>
                    <h3 class="luxury-policy-title">Royal Booking Policies</h3>
                </div>
                
                <!-- Policy List with Database Integration -->
                <div class="luxury-policy-list">
                    {{#if rates_content.booking_policies}}
                    {{#each (split rates_content.booking_policies " • ")}}
                    <div class="luxury-policy-item">
                        <div class="luxury-policy-ornament">♦</div>
                        <span class="luxury-policy-text">{{this}}</span>
                    </div>
                    {{/each}}
                    {{else}}
                    <div class="luxury-policy-item">
                        <div class="luxury-policy-ornament">♦</div>
                        <span class="luxury-policy-text">Advance booking recommended</span>
                    </div>
                    <div class="luxury-policy-item">
                        <div class="luxury-policy-ornament">♦</div>
                        <span class="luxury-policy-text">Same day booking: +$100</span>
                    </div>
                    <div class="luxury-policy-item">
                        <div class="luxury-policy-ornament">♦</div>
                        <span class="luxury-policy-text">Screening process required for all new clients</span>
                    </div>
                    {{/if}}
                </div>
            </div>
        </div>
        
        <!-- Payment & Terms Palace Section -->
        <div class="grid lg:grid-cols-2 gap-12 mt-12">
            <!-- Royal Payment Terms -->
            {{#if rates_content.rates_payment_visible}}
            <div id="payment-palace" class="luxury-payment-palace" data-aos="fade-up" data-aos-delay="300">
                <!-- Payment Palace Header -->
                <div class="luxury-payment-header">
                    <div class="luxury-payment-crown">
                        <i class="fas fa-credit-card"></i>
                    </div>
                    <h3 class="luxury-payment-title">Royal Payment Terms</h3>
                </div>
                
                <!-- Payment Terms with Database Integration -->
                <div class="luxury-payment-list">
                    {{#if rates_content.payment_terms}}
                    {{#each (split rates_content.payment_terms " • ")}}
                    <div class="luxury-payment-item">
                        <div class="luxury-payment-ornament">♦</div>
                        <span class="luxury-payment-text">{{this}}</span>
                    </div>
                    {{/each}}
                    {{else}}
                    <div class="luxury-payment-item">
                        <div class="luxury-payment-ornament">♦</div>
                        <span class="luxury-payment-text">Cash or discrete payment methods accepted</span>
                    </div>
                    <div class="luxury-payment-item">
                        <div class="luxury-payment-ornament">♦</div>
                        <span class="luxury-payment-text">Payment required at the beginning of our time together</span>
                    </div>
                    <div class="luxury-payment-item">
                        <div class="luxury-payment-ornament">♦</div>
                        <span class="luxury-payment-text">All transactions are handled with complete discretion</span>
                    </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
            
            <!-- Terms & Royal Conditions -->
            {{#if rates_content.terms_visible}}
            <div class="luxury-terms-palace" data-aos="fade-up" data-aos-delay="400">
                <!-- Terms Palace Header -->
                <div class="luxury-terms-header">
                    <div class="luxury-terms-crown">
                        <i class="fas fa-file-contract"></i>
                    </div>
                    <h3 class="luxury-terms-title">
                        {{#if rates_content.terms_title}}{{rates_content.terms_title}}{{else}}Royal Terms & Conditions{{/if}}
                    </h3>
                </div>
                
                <!-- Terms List with Database Integration -->
                <div class="luxury-terms-list">
                    {{#if rates_content.terms_content}}
                    {{#each (split rates_content.terms_content " • ")}}
                    <div class="luxury-terms-item">
                        <div class="luxury-terms-ornament">♦</div>
                        <span class="luxury-terms-text">{{this}}</span>
                    </div>
                    {{/each}}
                    {{else}}
                    <div class="luxury-terms-item">
                        <div class="luxury-terms-ornament">♦</div>
                        <span class="luxury-terms-text">All arrangements are for companionship and social time only</span>
                    </div>
                    <div class="luxury-terms-item">
                        <div class="luxury-terms-ornament">♦</div>
                        <span class="luxury-terms-text">Mutual respect and professionalism expected at all times</span>
                    </div>
                    <div class="luxury-terms-item">
                        <div class="luxury-terms-ornament">♦</div>
                        <span class="luxury-terms-text">Confidentiality and discretion are paramount</span>
                    </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
        </div>
    </div>
</section>

<!-- Royal Donations Palace -->
{{#if rates_content.donations_visible}}
<section class="luxury-donations-palace" data-aos="fade-in">
    <div class="max-w-5xl mx-auto px-4 sm:px-6 lg:px-8 text-center">
        <div class="luxury-donation-chamber">
            <!-- Donation Crown -->
            <div class="luxury-donation-crown">
                <i class="fas fa-heart"></i>
            </div>
            
            <h2 class="luxury-donation-title">
                {{#if rates_content.donation_title}}{{rates_content.donation_title}}{{else}}Royal Donations{{/if}}
            </h2>
            
            <p class="luxury-donation-description">
                {{#if rates_content.donation_description}}{{rates_content.donation_description}}{{else}}Donations are appreciated to help support my work{{/if}}
            </p>
            
            <!-- Golden Aura Effect -->
            <div class="luxury-donation-aura"></div>
        </div>
    </div>
</section>
{{/if}}

<!-- Royal CTA Palace Section -->
{{#if rates_content.cta_visible}}
<section id="contact-section" class="luxury-rates-cta-palace" data-aos="fade-in">
    <!-- Imperial Palace Background -->
    <div class="luxury-cta-imperial-bg">
        <div class="luxury-imperial-columns">
            <div class="luxury-column luxury-column-1"></div>
            <div class="luxury-column luxury-column-2"></div>
            <div class="luxury-column luxury-column-3"></div>
        </div>
    </div>
    
    <div class="max-w-5xl mx-auto text-center px-4 sm:px-6 lg:px-8">
        <!-- Imperial Crown -->
        <div class="luxury-cta-crown-imperial" data-aos="fade-down">
            <i class="fas fa-crown"></i>
        </div>
        
        <h2 class="luxury-cta-title-imperial" data-aos="fade-up" data-aos-delay="200">
            {{#if rates_content.cta_title}}{{rates_content.cta_title}}{{else}}Ready to Book?{{/if}}
        </h2>
        
        <p class="luxury-cta-subtitle-imperial" data-aos="fade-up" data-aos-delay="400">
            {{#if rates_content.cta_description}}{{rates_content.cta_description}}{{else}}Contact me to discuss your preferences and arrange our time together.{{/if}}
        </p>
        
        <!-- Enhanced CTA Secondary Text with Database Control -->
        {{#if rates_content.cta_secondary_text}}
        <p class="luxury-cta-secondary-text" data-aos="fade-up" data-aos-delay="500">
            {{rates_content.cta_secondary_text}}
        </p>
        {{/if}}
        
        <div class="luxury-cta-buttons-imperial" data-aos="fade-up" data-aos-delay="600">
            <!-- Primary CTA Button -->
            {{#if rates_content.cta_button_1_text}}
            <a href="/{{modelSlug}}/{{#if rates_content.cta_button_1_link}}{{rates_content.cta_button_1_link}}{{else}}contact{{/if}}{{previewParam}}" 
               class="luxury-btn luxury-btn-imperial luxury-crown-effect">
                <span>{{rates_content.cta_button_1_text}}</span>
                <div class="luxury-btn-imperial-glow"></div>
            </a>
            {{else}}
            <a href="/{{modelSlug}}/contact{{previewParam}}" 
               class="luxury-btn luxury-btn-imperial luxury-crown-effect">
                <span>Contact Me</span>
                <div class="luxury-btn-imperial-glow"></div>
            </a>
            {{/if}}
            
            <!-- Secondary CTA Button -->
            {{#if rates_content.cta_button_2_text}}
            <a href="/{{modelSlug}}/{{#if rates_content.cta_button_2_link}}{{rates_content.cta_button_2_link}}{{else}}about{{/if}}{{previewParam}}" 
               class="luxury-btn luxury-btn-royal luxury-crystal-border">
                <span>{{rates_content.cta_button_2_text}}</span>
            </a>
            {{else}}
            <a href="/{{modelSlug}}/about{{previewParam}}" 
               class="luxury-btn luxury-btn-royal luxury-crystal-border">
                <span>Learn More</span>
            </a>
            {{/if}}
        </div>
        
