<!-- Gallery Preview -->
{{#if content.gallerySectionVisible}}
<section class="py-16 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <h2 class="text-3xl font-bold text-center mb-12">
            {{#if content.gallerySectionTitle}}{{content.gallerySectionTitle}}{{else}}Gallery{{/if}}
        </h2>
        <div class="grid grid-cols-1 md:grid-cols-3 lg:grid-cols-5 gap-4">
            {{#each galleryImages}}
            <div class="aspect-square rounded-lg overflow-hidden bg-gray-200">
                <img src="/uploads/{{../modelSlug}}/public/gallery/{{this.filename}}" 
                     alt="{{#if this.caption}}{{this.caption}}{{else}}Gallery image{{/if}}" 
                     class="w-full h-full object-cover object-top">
            </div>
            {{else}}
            {{#times 5}}
            <div class="bg-gray-200 aspect-square rounded-lg flex items-center justify-center">
                <p class="text-gray-500 text-sm">Photo</p>
            </div>
            {{/times}}
            {{/each}}
        </div>
        <div class="text-center mt-8">
            <a href="/{{modelSlug}}/gallery{{previewParam}}" class="bg-blue-600 text-white px-8 py-3 rounded-lg font-semibold hover:bg-blue-700 transition-colors">
                {{#if content.galleryButtonText}}{{content.galleryButtonText}}{{else}}View Full Gallery{{/if}}
            </a>
        </div>
    </div>
</section>
{{/if}}

<!-- Upcoming Calendar Events -->
{{#if content.travelSectionVisible}}
<section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <h2 class="text-3xl font-bold text-center mb-12">
            {{#if content.travelSectionTitle}}{{content.travelSectionTitle}}{{else}}Travel Schedule{{/if}}
        </h2>
        {{#if upcomingEvents}}
        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
            {{#each upcomingEvents}}
            <div class="bg-gradient-to-br from-green-50 to-emerald-50 border border-green-200 rounded-xl p-6 hover:shadow-lg transition-shadow">
                <div class="text-center mb-4">
                    <div class="inline-flex items-center justify-center w-12 h-12 bg-green-500 text-white rounded-full mb-3">
                        <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                        </svg>
                    </div>
                    <h3 class="text-2xl font-bold text-green-900 mb-1">{{this.location}}</h3>
                    <div class="text-lg font-semibold text-green-700">
                        {{#if this.dateRange}}
                            {{this.dateRange}}
                        {{else}}
                            {{formatDate this.date}}
                        {{/if}}
                    </div>
                </div>
                <div class="text-center">
                    <span class="bg-green-500 text-white px-4 py-2 rounded-full text-sm font-bold">Available</span>
                </div>
                {{#if this.notes}}
                <p class="text-sm text-gray-600 text-center italic mt-3">{{this.notes}}</p>
                {{/if}}
            </div>
            {{/each}}
        </div>
        <div class="text-center mt-8">
            <a href="/{{modelSlug}}/{{#if content.travelCtaLink}}{{content.travelCtaLink}}{{else}}calendar{{/if}}{{previewParam}}" class="bg-blue-600 text-white px-8 py-3 rounded-lg font-semibold hover:bg-blue-700 transition-colors">
                {{#if content.travelCtaText}}{{content.travelCtaText}}{{else}}View Full Calendar{{/if}}
            </a>
        </div>
        {{else}}
        <div class="text-center">
            <div class="bg-gray-50 p-12 rounded-lg">
                <div class="w-16 h-16 mx-auto mb-6 bg-blue-600 rounded-lg flex items-center justify-center">
                    <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                    </svg>
                </div>
                <h3 class="text-xl font-semibold mb-4">Check My Calendar</h3>
                <p class="text-gray-600 mb-6">View available dates and book your appointment</p>
                <a href="/{{modelSlug}}/calendar{{previewParam}}" class="bg-blue-600 text-white px-8 py-3 rounded-lg font-semibold hover:bg-blue-700 transition-colors">
                    View Calendar
                </a>
            </div>
        </div>
        {{/if}}
    </div>
</section>
{{/if}}

