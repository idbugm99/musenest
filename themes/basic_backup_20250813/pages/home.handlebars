<!-- Hero Section -->
{{#if content.heroSectionVisible}}
<section class="relative {{#if heroBackgroundImageUrl}}bg-cover bg-center bg-fixed{{else}}bg-gradient-to-r from-blue-600 to-blue-800{{/if}} text-white py-20" 
         {{#if heroBackgroundImageUrl}}style="background-image: url('{{heroBackgroundImageUrl}}')"{{/if}}>
    {{#if heroBackgroundImageUrl}}
    <!-- Dynamic overlay for better text contrast -->
    <div class="absolute inset-0 bg-black" style="opacity: {{#if content.heroBackgroundOpacity}}{{content.heroBackgroundOpacity}}{{else}}0.6{{/if}};"></div>
    {{/if}}
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 {{#if heroBackgroundImageUrl}}relative z-10{{/if}}">
        <div class="text-center">
            <h1 class="text-4xl md:text-6xl font-bold mb-6">
                {{#if content.heroTitle}}{{content.heroTitle}}{{else}}{{siteName}}{{/if}}
            </h1>
            <p class="text-xl md:text-2xl mb-8 max-w-3xl mx-auto">
                {{#if content.heroSubtitle}}{{content.heroSubtitle}}{{else}}{{#if content.tagline}}{{content.tagline}}{{else}}Elegance and sophistication with discretion{{/if}}{{/if}}
            </p>
            <div class="space-x-4">
                <!-- Hero Button 1 -->
                {{#if content.heroButton1Text}}
                    {{#eq content.heroButton1Link 'calendar'}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton1Link 'contact'}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton1Link 'gallery'}}
                        <a href="/{{modelSlug}}/gallery{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton1Link 'rates'}}
                        <a href="/{{modelSlug}}/rates{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton1Link 'about'}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                    {{/eq}}
                {{else}}
                    <a href="/{{modelSlug}}/calendar{{previewParam}}" class="bg-white text-blue-600 px-8 py-3 rounded-lg font-semibold hover:bg-gray-100 transition-colors">
                {{/if}}
                    {{#if content.heroButton1Text}}{{content.heroButton1Text}}{{else}}View Calendar{{/if}}
                </a>
                
                <!-- Hero Button 2 -->
                {{#if content.heroButton2Text}}
                    {{#eq content.heroButton2Link 'calendar'}}
                        <a href="/{{modelSlug}}/calendar{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton2Link 'contact'}}
                        <a href="/{{modelSlug}}/contact{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton2Link 'gallery'}}
                        <a href="/{{modelSlug}}/gallery{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton2Link 'rates'}}
                        <a href="/{{modelSlug}}/rates{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                    {{/eq}}
                    {{#eq content.heroButton2Link 'about'}}
                        <a href="/{{modelSlug}}/about{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                    {{/eq}}
                {{else}}
                    <a href="/{{modelSlug}}/contact{{previewParam}}" class="border-2 border-white text-white px-8 py-3 rounded-lg font-semibold hover:bg-white hover:text-blue-600 transition-colors">
                {{/if}}
                    {{#if content.heroButton2Text}}{{content.heroButton2Text}}{{else}}Contact Me{{/if}}
                </a>
            </div>
        </div>
    </div>
</section>
{{/if}}

