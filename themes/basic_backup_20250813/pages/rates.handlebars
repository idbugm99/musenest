<!-- Rates Hero Section -->
<section class="bg-basic-primary text-white py-16" data-aos="fade-in">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center">
            <h1 class="text-4xl md:text-5xl font-bold mb-6 basic-heading">
                {{#if rates_content.page_title}}{{rates_content.page_title}}{{else}}Rates & Donations{{/if}}
            </h1>
            
            <!-- Optional Divider -->
            {{#if rates_content.hero_divider_visible}}
            <div class="mb-6">
                {{#if (eq rates_content.hero_divider_type 'line')}}
                <div class="w-24 h-1 bg-white opacity-50 mx-auto"></div>
                {{else}}
                <i class="{{#if rates_content.hero_divider_icon}}{{rates_content.hero_divider_icon}}{{else}}fas fa-heart{{/if}} text-2xl opacity-75"></i>
                {{/if}}
            </div>
            {{/if}}
            
            <p class="{{#if rates_content.hero_subtitle_size}}{{#if (eq rates_content.hero_subtitle_size 'small')}}text-xl md:text-2xl{{else}}{{#if (eq rates_content.hero_subtitle_size 'large')}}text-3xl md:text-4xl{{else}}text-2xl md:text-3xl{{/if}}{{/if}}{{else}}text-2xl md:text-3xl{{/if}} max-w-4xl mx-auto leading-relaxed font-light" style="color: {{#if rates_content.hero_subtitle_color}}{{rates_content.hero_subtitle_color}}{{else}}rgba(255,255,255,0.9){{/if}}; line-height: 1.6; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; text-overflow: ellipsis;">
                {{#if rates_content.page_subtitle}}{{rates_content.page_subtitle}}{{else}}Investment in time together. All arrangements are for companionship and social time only.{{/if}}
            </p>
        </div>
    </div>
</section>

<!-- Sticky Sub-Navigation -->
