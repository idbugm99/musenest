            </a>
            {{#if rates_content.rates_payment_visible}}
            <a href="#payment-section" class="nav-link text-sm font-medium text-gray-600 hover:text-basic-primary transition-colors px-3 py-2 rounded-lg hover:bg-gray-50">
                {{#if rates_content.nav_payment_label}}{{rates_content.nav_payment_label}}{{else}}Payment{{/if}}
            </a>
            {{/if}}
            {{#if rates_content.cta_visible}}
            <a href="#contact-section" class="nav-link text-sm font-medium text-gray-600 hover:text-basic-primary transition-colors px-3 py-2 rounded-lg hover:bg-gray-50">
                {{#if rates_content.nav_contact_label}}{{rates_content.nav_contact_label}}{{else}}Contact{{/if}}
            </a>
            {{/if}}
        </nav>
    </div>
</div>

<!-- Main Rates Section - Two Column Layout -->
{{#if rates_content.rates_table_visible}}
<section id="rates-section" class="py-20 bg-gray-50">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-8">
            
            <!-- Incall Rates -->
            {{#if rates_content.rates_incall_visible}}
            <div class="bg-white rounded-lg shadow-lg overflow-hidden" data-aos="fade-up" data-aos-delay="0"
                 {{#if rates_content.rates_background_tint_visible}}style="background-color: {{#if rates_content.rates_background_tint_color}}{{rates_content.rates_background_tint_color}}{{else}}#F8F9FA{{/if}};"{{/if}}>
                <div class="bg-blue-600 text-white px-6 py-4">
                    <h2 class="text-2xl font-bold text-center">Incall</h2>
                </div>
                <div class="p-6">
                    {{#each rates.incall}}
                    <div class="flex items-center py-4 border-b border-gray-200 last:border-b-0 relative">
                        <div class="flex-1 pr-4">
                            <div class="font-semibold text-gray-900 flex items-center">
                                {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                {{#if ../rates_content.rates_highlight_badge_visible}}
                                {{#if this.highlightBadge}}
                                <span class="ml-2 px-2 py-1 bg-yellow-400 text-yellow-900 text-xs font-bold rounded-full">
                                    {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}{{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}{{/if}}
                                </span>
                                {{else}}{{#if this.isMostPopular}}
                                <span class="ml-2 px-2 py-1 bg-yellow-400 text-yellow-900 text-xs font-bold rounded-full">
                                    {{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}
                                </span>
                                {{/if}}{{/if}}
                                {{/if}}
                            </div>
                            {{#if this.serviceName}}
                            <div class="text-sm text-gray-600">{{this.duration}}</div>
                            {{/if}}
                        </div>
                        
                        <!-- Dotted Leaders (Desktop Only) -->
                        <div class="hidden md:block flex-1 border-b border-dotted border-gray-300 mx-4"></div>
                        
                        <div class="text-xl font-bold text-blue-600">${{this.price}}</div>
                    </div>
                    {{/each}}
                </div>
            </div>
            {{/if}}

            <!-- Outcall Rates -->
            {{#if rates_content.rates_outcall_visible}}
            <div class="bg-white rounded-lg shadow-lg overflow-hidden" data-aos="fade-up" data-aos-delay="100"
                 {{#if rates_content.rates_background_tint_visible}}style="background-color: {{#if rates_content.rates_background_tint_color}}{{rates_content.rates_background_tint_color}}{{else}}#F8F9FA{{/if}};"{{/if}}>
                <div class="bg-amber-100 text-amber-900 px-6 py-4">
                    <h2 class="text-2xl font-bold text-center">Outcall</h2>
                </div>
                <div class="p-6">
                    {{#each rates.outcall}}
                    <div class="flex items-center py-4 border-b border-gray-200 last:border-b-0 relative">
                        <div class="flex-1 pr-4">
                            <div class="font-semibold text-gray-900 flex items-center">
                                {{#if this.serviceName}}{{this.serviceName}}{{else}}{{this.duration}}{{/if}}
                                {{#if ../rates_content.rates_highlight_badge_visible}}
                                {{#if this.highlightBadge}}
                                <span class="ml-2 px-2 py-1 bg-yellow-400 text-yellow-900 text-xs font-bold rounded-full">
                                    {{#if this.highlightBadgeText}}{{this.highlightBadgeText}}{{else}}{{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}{{/if}}
                                </span>
                                {{else}}{{#if this.isMostPopular}}
                                <span class="ml-2 px-2 py-1 bg-yellow-400 text-yellow-900 text-xs font-bold rounded-full">
                                    {{#if ../rates_content.rates_highlight_badge_text}}{{../rates_content.rates_highlight_badge_text}}{{else}}Most Popular{{/if}}
                                </span>
                                {{/if}}{{/if}}
                                {{/if}}
                            </div>
                            {{#if this.serviceName}}
                            <div class="text-sm text-gray-600">{{this.duration}}</div>
                            {{/if}}
                        </div>
                        
                        <!-- Dotted Leaders (Desktop Only) -->
                        <div class="hidden md:block flex-1 border-b border-dotted border-gray-300 mx-4"></div>
                        
                        <div class="text-xl font-bold text-amber-700">${{this.price}}</div>
                    </div>
                    {{/each}}
                </div>
            </div>
            {{/if}}
        </div>
    </div>
</section>
{{/if}}

<!-- Extended Engagements Section -->
{{#if rates_content.rates_extended_visible}}
{{#if rates.extended.length}}
<section id="extended-section" class="py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12" data-aos="fade-up">
            <h2 class="text-3xl font-bold mb-4 basic-heading text-basic-primary">
                Extended Engagements
            </h2>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
            {{#each rates.extended}}
            <div class="{{#if ../rates_content.extended_color_saturation}}{{#if (eq ../rates_content.extended_color_saturation 'low')}}bg-blue-300{{else}}{{#if (eq ../rates_content.extended_color_saturation 'high')}}bg-blue-600{{else}}bg-blue-400{{/if}}{{/if}}{{else}}bg-blue-400{{/if}} text-white rounded-xl shadow-lg p-8 text-center transition-all hover:scale-105 hover:shadow-xl" data-aos="fade-up" data-aos-delay="{{multiply @index 100}}">
                <div class="flex items-center justify-center mb-4">
                    {{#if ../rates_content.extended_icon_visible}}
                    <div class="w-12 h-12 mr-3 bg-white bg-opacity-20 rounded-full flex items-center justify-center">
                        <i class="{{#if this.rateIcon}}{{this.rateIcon}}{{else}}{{#eq this.serviceName 'Weekend'}}fas fa-calendar-weekend{{else}}{{#eq this.serviceName 'Week'}}fas fa-calendar-week{{else}}fas fa-plane{{/eq}}{{/eq}}{{/if}} text-lg text-white"></i>
                    </div>
                    {{/if}}
                    <h3 class="text-2xl font-bold">{{this.serviceName}}</h3>
                </div>
                <div class="text-4xl font-bold mb-2">${{this.price}}</div>
                <p class="text-white opacity-90 mb-3 font-medium">{{this.duration}}</p>
                {{#if ../rates_content.extended_description_visible}}
                {{#if this.rateDescription}}
                <p class="text-white text-sm opacity-75 leading-relaxed max-w-xs mx-auto">{{this.rateDescription}}</p>
                {{/if}}
                {{/if}}
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/if}}
{{/if}}

<!-- Detail Sections - Two Column Layout -->
<section id="services-section" class="py-20 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-8">
            
            <!-- Additional Services -->
            {{#if rates_content.rates_additional_visible}}
            <div class="bg-white rounded-lg shadow-lg p-8" data-aos="fade-up" data-aos-delay="0">
                <div class="text-center mb-6">
                    <i class="fas fa-plus-circle text-3xl text-purple-600 mb-4"></i>
                    <h3 class="text-2xl font-bold text-gray-900">Additional Services</h3>
                </div>
                
                {{#if rates_content.additional_services}}
                    <ul class="space-y-3">
                        {{#each (split rates_content.additional_services " • ")}}
                        <li class="flex items-start">
                            <i class="fas fa-plus-circle text-purple-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">{{this}}</span>
                        </li>
                        {{/each}}
                    </ul>
                {{else}}
                    <ul class="space-y-3">
                        <li class="flex items-start">
                            <i class="fas fa-plus-circle text-purple-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Dinner dates: +$100 (restaurant cost separate)</span>
                        </li>
                        <li class="flex items-start">
                            <i class="fas fa-plus-circle text-purple-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Travel outside city: +$500/day + expenses</span>
                        </li>
                    </ul>
                {{/if}}
            </div>
            {{/if}}
            
            <!-- Booking Policies -->
            <div id="policies-section" class="bg-white rounded-lg shadow-lg p-8" data-aos="fade-up" data-aos-delay="100">
                <div class="text-center mb-6">
                    <i class="fas fa-calendar-check text-3xl text-green-600 mb-4"></i>
                    <h3 class="text-2xl font-bold text-gray-900">Booking Policies</h3>
                </div>
                
                {{#if rates_content.booking_policies}}
                    <ul class="space-y-3">
                        {{#each (split rates_content.booking_policies " • ")}}
                        <li class="flex items-start">
                            <i class="fas fa-calendar-check text-green-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">{{this}}</span>
                        </li>
                        {{/each}}
                    </ul>
                {{else}}
                    <ul class="space-y-3">
                        <li class="flex items-start">
                            <i class="fas fa-calendar-check text-green-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Advance booking recommended</span>
                        </li>
                        <li class="flex items-start">
                            <i class="fas fa-calendar-check text-green-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Same day booking: +$100</span>
                        </li>
                    </ul>
                {{/if}}
            </div>
        </div>
        
        <!-- Payment Terms and Terms Grid -->
        <div class="mt-8 grid grid-cols-1 lg:grid-cols-2 gap-8">
            <!-- Payment Terms -->
            {{#if rates_content.rates_payment_visible}}
            <div id="payment-section" class="bg-white rounded-lg shadow-lg p-8" data-aos="fade-up" data-aos-delay="200">
                <div class="text-center mb-6">
                    <i class="fas fa-credit-card text-3xl text-blue-600 mb-4"></i>
                    <h3 class="text-2xl font-bold text-gray-900">Payment Terms</h3>
                </div>
                
                <div class="space-y-3">
                    {{#if rates_content.payment_terms}}
                        {{#each (split rates_content.payment_terms " • ")}}
                        <div class="flex items-start">
                            <i class="fas fa-credit-card text-blue-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">{{this}}</span>
                        </div>
                        {{/each}}
                    {{else}}
                        <div class="flex items-start">
                            <i class="fas fa-credit-card text-blue-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Cash or discrete payment methods accepted</span>
                        </div>
                        <div class="flex items-start">
                            <i class="fas fa-credit-card text-blue-600 mr-3 mt-1 flex-shrink-0"></i>
                            <span class="text-gray-700">Payment required at the beginning of our time together</span>
                        </div>
                    {{/if}}
                </div>
            </div>
            {{/if}}
            
            <!-- Terms & Additional Information -->
            {{#if rates_content.terms_visible}}
            <div class="bg-white rounded-lg shadow-lg p-8" data-aos="fade-up" data-aos-delay="300">
                <div class="text-center mb-6">
                    <i class="fas fa-file-contract text-3xl text-gray-600 mb-4"></i>
                    <h3 class="text-2xl font-bold text-gray-900">
                        {{#if rates_content.terms_title}}{{rates_content.terms_title}}{{else}}Terms & Conditions{{/if}}
                    </h3>
                </div>
                <div class="space-y-3">
                    {{#each (split rates_content.terms_content " • ")}}
                    <div class="flex items-start">
                        <i class="fas fa-file-contract text-gray-600 mr-3 mt-1 flex-shrink-0"></i>
                        <span class="text-gray-700">{{this}}</span>
                    </div>
                    {{/each}}
                </div>
            </div>
            {{/if}}
        </div>
    </div>
</section>

