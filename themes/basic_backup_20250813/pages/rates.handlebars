                {{#if rates_content.nav_rates_label}}{{rates_content.nav_rates_label}}{{else}}Rates{{/if}}
            </a>
            {{#if rates.extended.length}}
            <a href="#extended-section" class="nav-link text-sm font-medium text-gray-600 hover:text-basic-primary transition-colors px-3 py-2 rounded-lg hover:bg-gray-50">
                {{#if rates_content.nav_extended_label}}{{rates_content.nav_extended_label}}{{else}}Extended{{/if}}
            </a>
            {{/if}}
            {{#if rates_content.rates_additional_visible}}
            <a href="#services-section" class="nav-link text-sm font-medium text-gray-600 hover:text-basic-primary transition-colors px-3 py-2 rounded-lg hover:bg-gray-50">
                {{#if rates_content.nav_services_label}}{{rates_content.nav_services_label}}{{else}}Services{{/if}}
            </a>
            {{/if}}
            <a href="#policies-section" class="nav-link text-sm font-medium text-gray-600 hover:text-basic-primary transition-colors px-3 py-2 rounded-lg hover:bg-gray-50">
