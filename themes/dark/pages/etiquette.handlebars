<!-- Dark Etiquette Protocol Initialization JavaScript -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    // Protocol terminal initialization
    const protocolTerminals = document.querySelectorAll('.dark-protocol-terminal');
    
    protocolTerminals.forEach((terminal, index) => {
        // Add initialization delay for each terminal
        setTimeout(() => {
            terminal.classList.add('dark-initialized');
            
            // Add scanner effect to protocol cards within this terminal
            const protocolCards = terminal.querySelectorAll('.dark-protocol-card');
            protocolCards.forEach((card, cardIndex) => {
                setTimeout(() => {
                    card.classList.add('dark-protocol-active');
                }, cardIndex * 100);
            });
        }, index * 200);
    });
    
    // Protocol card hover enhancement
    const protocolCards = document.querySelectorAll('.dark-protocol-card');
    protocolCards.forEach(card => {
        card.addEventListener('mouseenter', function() {
            // Add scanner line effect
            const scanner = this.querySelector('.dark-data-stream');
            if (scanner) {
                scanner.style.animationDuration = '1s';
            }
        });
        
        card.addEventListener('mouseleave', function() {
            // Reset scanner timing
            const scanner = this.querySelector('.dark-data-stream');
            if (scanner) {
                scanner.style.animationDuration = '2s';
            }
        });
    });
    
    // Status indicator pulse synchronization
    const statusIndicators = document.querySelectorAll('.dark-status-indicator');
    statusIndicators.forEach(indicator => {
        // Random pulse delay for cyberpunk effect
        const delay = Math.random() * 2;
        indicator.style.animationDelay = `${delay}s`;
    });
    
    // Terminal boot sequence simulation
    setTimeout(() => {
        const terminals = document.querySelectorAll('.dark-terminal-header');
        terminals.forEach((header, index) => {
            setTimeout(() => {
                header.classList.add('dark-terminal-booted');
            }, index * 300);
        });
    }, 1000);
});

// Protocol acknowledgment system
function acknowledgeProtocol(protocolName) {
    console.log(`PROTOCOL_ACK: ${protocolName} acknowledged by user agent`);
    
    // Create acknowledgment notification
    const ackDiv = document.createElement('div');
    ackDiv.style.cssText = `
        position: fixed;
        top: 20px;
        right: 20px;
        background: rgba(0, 0, 0, 0.9);
        border: 1px solid #00f5ff;
        color: #00f5ff;
        padding: 1rem;
        z-index: 9999;
        font-family: 'Courier New', monospace;
        font-size: 0.875rem;
        max-width: 300px;
        box-shadow: 0 0 20px rgba(0, 245, 255, 0.3);
    `;
    
    ackDiv.innerHTML = `
        <div style="font-weight: bold; margin-bottom: 0.5rem; text-transform: uppercase; letter-spacing: 0.1em;">PROTOCOL_ACKNOWLEDGED</div>
        <div>${protocolName} guidelines confirmed and integrated into user session.</div>
    `;
    
    document.body.appendChild(ackDiv);
    
    // Auto remove after 3 seconds
    setTimeout(() => {
        if (ackDiv.parentNode) {
            ackDiv.parentNode.removeChild(ackDiv);
        }
    }, 3000);
}

// Auto-acknowledge protocols on page load
setTimeout(() => {
    acknowledgeProtocol('DIGITAL_PROTOCOL_DATABASE');
}, 2000);
</script>